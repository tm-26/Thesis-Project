[{"abstract": "Standard Life, based in Edinburgh, will acquire Ignis Asset Management from Phoenix Group Holdings in a deal that will increase its investment capabilities and broaden its third-party client base.", "web_url": "https://dealbook.nytimes.com/2014/03/26/britains-standard-life-to-pay-644-million-for-asset-manager/", "snippet": "Standard Life, based in Edinburgh, will acquire Ignis Asset Management from Phoenix Group Holdings in a deal that will increase its investment capabilities and broaden its third-party client base.", "lead_paragraph": "LONDON \u2013 The investment arm of insurer Standard Life said on Wednesday that it had agreed to pay 390 million pounds, or about $645 million, for the asset management unit of Phoenix Group Holdings.", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2014/03/27/business/dbpix-standard-life/dbpix-standard-life-articleLarge.jpg", "height": 399, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2014/03/27/business/dbpix-standard-life/dbpix-standard-life-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 399}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2014/03/27/business/dbpix-standard-life/dbpix-standard-life-jumbo.jpg", "height": 681, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2014/03/27/business/dbpix-standard-life/dbpix-standard-life-superJumbo.jpg", "height": 1363, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2014/03/27/business/dbpix-standard-life/dbpix-standard-life-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thum