[{"abstract": "The company\u2019s new device, called Amazon Fire TV, will marry its video library with content from other providers.", "web_url": "https://www.nytimes.com/2014/04/03/technology/amazon-firetv-set-top-box.html", "snippet": "The company\u2019s new device, called Amazon Fire TV, will marry its video library with content from other providers.", "lead_paragraph": "Amazon is a retailer that makes and sells entertainment. On Wednesday, it took a big step toward a future in which shopping and video are tightly linked, perhaps even inseparable.", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [], "headline": {"main": "With Fire TV, Amazon Seeks a Beachhead in the Living Room", "kicker": null, "content_kicker": null, "print_headline": "Amazon Seeks Beachhead in the Living Room", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Streitfeld, David", "rank": 1, "major": "N"}, {"name": "organizations", "value": "AMAZON.COM INC", "rank": 2, "major": "N"}, {"name": "subject", "value": "Television", "rank": 3, "major": "N"}, {"name": "subject", "value": "E-Commerce", "rank": 4, "major": "N"}, {"name": "subject", "value": "Computer and Video Games", "rank": 5, "major": "N"}, {"name": "subject", "value": "Television Sets", "rank": 6, "major": "N"}], "pub_date": "2014-04-02T16:01:24+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By David Streitfeld", "person": [{"firstname": "David", "middlename": null, "lastname": "Streitfeld", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/48ce2dec-2ef2-5731-ae7d-a17c796fe09f", "word_count": 1259, "uri": "nyt://article/48ce2dec-2ef2-5731-ae7d-a17c796fe09f"}, {"abstract": "The just announced device will stream Internet video to your television. But so will Internet-connected TVs and a host of competing devices. Can it stand out from the crowd?", "web_url": "https://bits.blogs.nytimes.com/2014/04/02/where-the-amazon-fire-tv-fits-into-the-living-room/", "snippet": "The just announced device will stream Internet video to your television. But so will Internet-connected TVs and a host of competing devices. Can it stand out from the crowd?", "lead_paragraph": "Amazon on Wednesday announced its very own streaming media box, the Amazon Fire TV. It costs $99 and will stream Netflix, Amazon Instant (of course), Hulu Plus, Showtime Anytime, Crackle and Vimeo, and more to be named.", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2014/04/02/technology/personaltech/02Amazon/02Amazon-articleLarge-v2.jpg", "height": 400, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2014/04/02/technology/personaltech/02Amazon/02Amazon-articleLarge-v2.jpg", "xlargewidth": 600, "xlargeheight": 400}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2014/04/02/technology/personaltech/02Amazon/02Amazon-jumbo-v2.jpg", "height": 683, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2014/04/02/technology/personaltech/02Amazon/02Amazon-superJumbo-v2.jpg", "height": 1366, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2014/04/02/technology/personaltech/02Amazon/02Amazon-thumbStandard-v3.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2014/04/02/technology/personaltech/02Amazon/02Amazon-thumbStandard-v3.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtyp