[{"abstract": "An abundance of opinions in nearly 5,000 comments on Apple\u2019s opposition to a court order to unlock an iPhone used by one of the San Bernardino killers.", "web_url": "https://www.nytimes.com/2016/02/19/technology/apple-fbi-reaction.html", "snippet": "An abundance of opinions in nearly 5,000 comments on Apple\u2019s opposition to a court order to unlock an iPhone used by one of the San Bernardino killers.", "lead_paragraph": "The tech industry\u2019s long-running dispute with the government over encryption practices flared when Apple this week opposed a court order for the technology company to unlock an iPhone used by one of the San Bernardino, Calif., killers.", "print_section": "B", "print_page": "7", "source": "The New York Times", "multimedia": [], "headline": {"main": "Times Readers Weigh In on the Apple Debate", "kicker": null, "content_kicker": null, "print_headline": "Times Readers Have Their Say on the Debate Over Apple", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Apple Inc", "rank": 1, "major": "N"}, {"name": "persons", "value": "McDermott, Marie Tae", "rank": 2, "major": "N"}, {"name": "subject", "value": "Privacy", "rank": 3, "major": "N"}, {"name": "subject", "value": "Search and Seizure", "rank": 4, "major": "N"}, {"name": "subject", "value": "San Bernardino, Calif, Shooting (2015)", "rank": 5, "major": "N"}, {"name": "organizations", "value": "New York Times", "rank": 6, "major": "N"}, {"name": "subject", "value": "iPhone", "rank": 7, "major": "N"}, {"name": "subject", "value": "Surveillance of Citizens by Government", "rank": 8, "major": "N"}, {"name": "organizations", "value": "Federal Bureau of Investigation", "rank": 9, "major": "N"}, {"name": "subject", "value": "Computer Security", "rank": 10, "major": "N"}, {"name": "organizations", "value": "Facebook Inc", "rank": 11, "major": "N"}, {"name": "persons", "value": "Cook, Timothy D", "rank": 12, "major": "N"}, {"name": "subject", "v