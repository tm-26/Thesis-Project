[{"abstract": "Apple\u2019s iPad seems to be hated by techies and loved by everyone else. Here are separate reviews for the two audiences.    ", "web_url": "https://www.nytimes.com/2010/04/01/technology/personaltech/01pogue.html", "snippet": "Apple\u2019s iPad seems to be hated by techies and loved by everyone else. Here are separate reviews for the two audiences.    ", "lead_paragraph": "In 10 years of reviewing tech products for The New York Times, I\u2019ve never seen a product as polarizing as Apple\u2019s iPad, which arrives in stores on Saturday.", "print_section": "A", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2010/04/01/technology/personaltech/01pogue2/01pogue2-articleLarge.jpg", "height": 315, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2010/04/01/technology/personaltech/01pogue2/01pogue2-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 315}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2010/04/01/technology/personaltech/01pogue2/01pogue2-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2010/04/01/technology/personaltech/01pogue2/01pogue2-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Looking at the iPad From Two Angles", "kicker": "State of the Art", "content_kicker": null, "print_headline": "Love It or Not? Looking at iPad From 2 Angles", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Apple Inc", "rank": 1, "major": "N"}, {"name": "subject", "value": "iPhone", "rank": 2, "major": "N"}, {"name": "subject", "value": "iPad", "rank": 3, "major": "N"}, {"name": "subject", "value": "Tablet Computers", "rank": 4, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 5, "major": "N"}], "pub_date": "2010-04-01T01:00:59+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "subsection_name": "Personal Tech", "byline": {"original": "By David Pogue", "person": [{"firstname": "David", "middlename": null, "lastname": "Pogue", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/6b176c6d-e1d9-5d3e-96db-19cad04990d7", "word_count": 1425, "uri": "nyt://article/6b176c6d-e1d9-5d3e-96db-19cad04990d7"}, {"abstract": "Hulu, the popular and free online video hub, has some things to celebrate as it heads into its third year, The New York Times writes.", "web_url": "https://dealbook.nytimes.com/2010/04/01/successes-and-some-growing-pains-at-hulu/", "snippet": "Hulu, the popular and free online video hub, has some things to celebrate as it heads into its third year, The New York Times writes.", "lead_paragraph": "Hulu, the popular and free online video hub, has some things to celebrate as it heads into its third year, The New York Times\u2019s Brian Stelter and Brad Stone write.", "source": "The New York Times", "multimedia": [], "headline": {"main": "Successes (and Some Growing Pains) at Hulu", "kicker": "DealBook", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Apple Inc", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Boxee Inc", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Comcast Corp", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Comedy Central", "rank": 4, "major": "N"}, {"name": "organizations", "value": "Disney, Walt, Co", "rank": 5, "major": "N"}, {"name": "organizations", "value": "FX", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Hulu.com", "rank": 7, "major": "N"}, {"name": "organizations", "value": "NBC Universa