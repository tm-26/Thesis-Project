[{"abstract": "If an iPod costs $299, who gets that money? You might be surprised.", "web_url": "https://www.nytimes.com/2007/06/28/business/worldbusiness/28scene.html", "snippet": "If an iPod costs $299, who gets that money? You might be surprised.", "lead_paragraph": "Who makes the Apple iPod? Here\u2019s a hint: It is not Apple. The company outsources the entire manufacture of the device to a number of Asian enterprises, among them Asustek, Inventec Appliances and Foxconn.", "print_section": "C", "print_page": "3", "source": "The New York Times", "multimedia": [], "headline": {"main": "An iPod Has Global Value. Ask the (Many) Countries That Make It.", "kicker": "Economic Scene", "content_kicker": null, "print_headline": "An iPod Has Global Value. Ask the (Many) Countries That Make It.", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Apple Inc", "rank": 1, "major": "N"}, {"name": "subject", "value": "Economic Conditions and Trends", "rank": 2, "major": "N"}, {"name": "subject", "value": "International Trade and World Market", "rank": 3, "major": "N"}], "pub_date": "2007-06-28T04:00:00+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "World Business", "byline": {"original": "By Hal R. Varian", "person": [{"firstname": "Hal", "middlename": "R.", "lastname": "Varian", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/2178d57c-62e1-5f6f-9bc0-debcf89406f1", "word_count": 935, "uri": "nyt://article/2178d57c-62e1-5f6f-9bc0-debcf89406f1"}, {"abstract": "Just how much of a phone, an iPod and an Internet machine is this thing?", "web_url": "https://www.nytimes.com/2007/06/28/technology/circuits/28pogue.html", "snippet": "Just how much of a phone, an iPod and an Internet machine is this thing?", "lead_paragraph": "With its new iPhone, Apple pulled off two masterfu