[{"abstract": "Apple has shielded billions in cash from federal  taxes in overseas accounts, drawing Senate scrutiny.", "web_url": "https://www.nytimes.com/2013/05/21/business/senate-panel-is-expected-to-castigate-apple-on-tax-tactics.html", "snippet": "Apple has shielded billions in cash from federal  taxes in overseas accounts, drawing Senate scrutiny.", "lead_paragraph": "Members of the United States Senate are expected to sharply criticize Apple\u2019s global tax tactics while questioning the company\u2019s chief executive, Timothy D. Cook, at a Congressional hearing on Tuesday.", "source": "The New York Times", "multimedia": [], "headline": {"main": "Senate Panel Is Expected to Castigate Apple on Tax Tactics", "kicker": null, "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Apple Inc", "rank": 1, "major": "N"}, {"name": "subject", "value": "Corporate Taxes", "rank": 2, "major": "N"}, {"name": "subject", "value": "Tax Shelters", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Permanent Subcommittee on Investigations", "rank": 4, "major": "N"}, {"name": "persons", "value": "Cook, Timothy D", "rank": 5, "major": "N"}, {"name": "persons", "value": "McCain, John", "rank": 6, "major": "N"}, {"name": "persons", "value": "Levin, Carl M", "rank": 7, "major": "N"}, {"name": "organizations", "value": "Senate", "rank": 8, "major": "N"}], "pub_date": "2013-05-20T20:51:22+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "byline": {"original": "By Charles Duhigg and David Kocieniewski", "person": [{"firstname": "Charles", "middlename": null, "lastname": "Duhigg", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}, {"firstname": "David", "middlename": null, "lastname": "Kocieniewski", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 2}], "organization": null}, "type_of_