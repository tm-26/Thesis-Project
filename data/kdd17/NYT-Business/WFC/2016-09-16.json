[{"abstract": "Former employees say that their managers warned them not to bend the rules, but they felt pressured by the bank\u2019s aggressive sales culture to create fake accounts anyway.", "web_url": "https://www.nytimes.com/2016/09/17/business/dealbook/wells-fargo-warned-workers-against-fake-accounts-but-they-needed-a-paycheck.html", "snippet": "Former employees say that their managers warned them not to bend the rules, but they felt pressured by the bank\u2019s aggressive sales culture to create fake accounts anyway.", "lead_paragraph": "The message to the dozens of Wells Fargo workers gathered for a two-day ethics workshop in San Diego in mid-2014 was loud and clear: Do not create fake bank accounts in the name of unsuspecting clients.", "print_section": "A", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2016/09/16/business/17DB-WELLS/17DB-WELLS-articleLarge.jpg", "height": 400, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2016/09/16/business/17DB-WELLS/17DB-WELLS-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 400}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2016/09/16/business/17DB-WELLS/17DB-WELLS-jumbo.jpg", "height": 682, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2016/09/16/business/17DB-WELLS/17DB-WELLS-superJumbo.jpg", "height": 1364, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2016/09/16/business/17DB-WELLS/17DB-WELLS-thumbStandard-v2.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2016/09/16/business/17DB-WELLS/17DB-WELLS-thumbStandard-v2.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtype": "thumbLarge", "caption": null, "credit": null, "type": "image", "url": "images/2016/09/16/business/17DB-WELLS/17DB-WELLS-thumbLarge-v2.jpg", "height": 150, "width": 150, "subType": "thumbLarge", "crop_name": "thumbLarge", "legacy": {}}], "headline": {"main": "Wells Fargo Warned Workers Against Sham Accounts, but \u2018They Needed a Paycheck\u2019", "kicker": null, "content_kicker": null, "print_headline": "Warned About Excesses, Then Prodded to Sell", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Wells Fargo & Company", "rank": 1, "major": "N"}, {"name": "persons", "value": "Cowley, Stacy", "rank": 2, "major": "N"}, {"name": "subject", "value": "Banking and Financial Institutions", "rank": 3, "major": "N"}, {"name": "persons", "value": "Corkery, Michael", "rank": 4, "major": "N"}, {"name": "subject", "value": "Frauds and Swindling", "rank": 5, "major": "N"}, {"name": "persons", "value": "Stumpf, John G", "rank": 6, "major": "N"}], "pub_date": "2016-09-16T16:19:31+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "DealBook", "byline": {"original": "By Michael Corkery and Stacy Cowley", "person": [{"firstname": "Michael", "middlename": null, "lastname": "Corkery", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}, {"firstname": "Stacy", "middlename": null, "lastname": "Cowley", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 2}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/8ac2d715-daba-51c8-8cf6-ad88b4c23a9d", "word_count": 1628, "uri": "nyt://article/8ac2d715-daba-51c8-8cf6-ad88b4c23a9d"}, {"abstract": "That senior executives were not held accountable for the reprehensible behavior of bank employees signals a wider cultural cancer on Wall Street.", "web_url": "https://www.nytimes.com/2016/09/17/business/dealbook/wells-fargo-scandal-may-be-sign-of-a-poisonous-culture.html", "snippet": "That senior executives were not held accountable for the reprehensible behavior of bank employees signals a wider cultural cancer on Wall Street.", "lead_paragraph": "Let me get this straight: Some 5,300 employees and managers at Wells Fargo\u2019s retail banking division knowingly opened millions of unauthorized accounts in the names of their customers, are rightly fired as a result, but the senior executives at the bank responsible for them and their reprehensible behavior are not? What year is this? 1928? 2007? Or is this 2016, eight years after the crucible moments leading up to one of the worst financial crises in our collective lifetimes?", "source": "The New York Times", "multimedia": [], "headline": {"main": "Wells Fargo Scandal May Be Sign of a Poisonous Culture", "kicker": "Street Scene", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Consumer Protection", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Wells Fargo & Company", "rank": 2, "major": "N"}, {"name": "persons", "value": "Stumpf, John G", "rank": 3, "major": "N"}, {"name": "persons", "value": "Tolstedt, Carrie L", "rank": 4, "major": "N"}], "pub_date": "2016-09-16T18:13:31+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "DealBook", "byline": {"original": "By William D. Cohan", "person": [{"firstname": "William", "middlename": null, "lastname": "", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/b3ea4acd-7f9a-56f6-97c3-4fd63056a41b", "word_count": 1020, "uri": "nyt://article/b3ea4acd-7f9a-56f6-97c3-4fd63056a41b"}]