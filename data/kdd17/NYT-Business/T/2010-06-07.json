[{"abstract": "As AT&T adopts tiered pricing, developers wonder whether consumers will lose their appetite for innovative features and bandwidth-heavy video.", "web_url": "https://www.nytimes.com/2010/06/07/technology/07data.html", "snippet": "As AT&T adopts tiered pricing, developers wonder whether consumers will lose their appetite for innovative features and bandwidth-heavy video.", "lead_paragraph": "For the last two years, unlimited data plans have given app-hungry smartphone users an all-you-can-eat buffet. But will customers react to AT&T\u2019s new, limited menu by simply eating less? ", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2010/06/07/business/data/data-articleLarge.jpg", "height": 330, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2010/06/07/business/data/data-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 330}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2010/06/07/business/data/data-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2010/06/07/business/data/data-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "App Makers Worry as Data Plans Are Capped", "kicker": null, "content_kicker": null, "print_headline": "As Unlimited Data Plans Are Challenged, App Developers Worry", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Software", "rank": 1, "major": "N"}, {"name": "subject", "value": "Wireless Communications", "rank": 2, "major": "N"}, {"name": "subject", "value": "iPhone", "rank": 3, "major": "N"}, {"name": "organizations", "value": "AT&T Corp", "rank": 4, "major": "N"}, {"name": "subject", "value": "Recordings and Downloads (Video)", "rank