[{"abstract": "Microsoft is joining a chorus of complaints saying that Google\u2019s search engine unfairly promotes its own products, like Google Product Search.", "web_url": "https://www.nytimes.com/2011/03/31/technology/companies/31google.html", "snippet": "Microsoft is joining a chorus of complaints saying that Google\u2019s search engine unfairly promotes its own products, like Google Product Search.", "lead_paragraph": "The wheel of technology history turns remarkably fast. Microsoft, whose domination of the technology industry provoked a landmark federal antitrust case, is crying foul against Google and urging European Union antitrust officials to go after the search giant. ", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2011/03/31/business/31GOOGLE/31GOOGLE-articleLarge.jpg", "height": 350, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2011/03/31/business/31GOOGLE/31GOOGLE-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 350}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/03/31/business/31GOOGLE/31GOOGLE-jumbo.jpg", "height": 755, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2011/03/31/business/31GOOGLE/31GOOGLE-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2011/03/31/business/31GOOGLE/31GOOGLE-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Antitrust Cry From Microsoft", "kicker": null, "content_kicker": null, "print_headline": "Antitrust Cry From Microsoft", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Microsoft Corp", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 2, "major": "N"}, {"name": "subject", "value": "Search Engines", "rank": 3, "major": "N"}, {"name": "glocations", "value": "Europe", "rank": 4, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 5, "major": "N"}, {"name": "subject", "value": "Antitrust Laws and Competition Issues", "rank": 6, "major": "N"}], "pub_date": "2011-03-31T04:00:20+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "subsection_name": "Companies", "byline": {"original": "By Steve Lohr", "person": [{"firstname": "Steve", "middlename": null, "lastname": "Lohr", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/20e24f16-8f43-5bcd-8972-68a1084c630a", "word_count": 1323, "uri": "nyt://article/20e24f16-8f43-5bcd-8972-68a1084c630a"}, {"abstract": "The European Commission said its investigation was still preliminary, and Google would be given a chance to respond to Microsoft's charges.", "web_url": "https://www.nytimes.com/2011/04/01/technology/01google.html", "snippet": "The European Commission said its investigation was still preliminary, and Google would be given a chance to respond to Microsoft's charges.", "lead_paragraph": "BRUSSELS \u2014 Google played down the significance of an antitrust complaint filed by one of its main rivals, Microsoft, on Thursday and said its discussions were continuing with European Union regulators.", "source": "The New York Times", "multimedia": [], "headline": {"main": "Google Plays Down Microsoft's Antitrust Complaint in Europe", "kicker": null, "content_kicker": null, "print_headline": "Google Plays Down Microsoft's Antitrust Complaint in Europe", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Google Inc", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Microsoft Corp", "rank": 2, "major": "N"}, {"name": "organizations", "value": "European Commission", "rank": 3, "major": "N"}, {"name": "subject", "value": "Antitrust Laws and Competition Issues", "rank": 4, "major": "N"}], "pub_date": "2011-03-31T12:54:00+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By James Kanter", "person": [{"firstname": "James", "middlename": null, "lastname": "Kanter", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/ad4abb35-24c8-50ea-8e0f-a37edc531c66", "word_count": 513, "uri": "nyt://article/ad4abb35-24c8-50ea-8e0f-a37edc531c66"}, {"abstract": "Maurice L\u00e9vy, chief of Publicis Groupe, said the company\u2019s expertise in digital marketing, had helped it to win business from technology companies like Microsoft.", "web_url": "https://www.nytimes.com/2011/04/01/business/media/01adco.html", "snippet": "Maurice L\u00e9vy, chief of Publicis Groupe, said the company\u2019s expertise in digital marketing, had helped it to win business from technology companies like Microsoft.", "lead_paragraph": "WHEN Microsoft last month awarded a big portion of its North American advertising account to the Publicis Groupe, the marketing company based in Paris, the news felt like vindication for its chief executive, Maurice L\u00e9vy. ", "print_section": "B", "print_page": "8", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/04/01/business/Adco/Adco-jumbo.jpg", "height": 665, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}], "headline": {"main": "At Publicis, a Big Bet on Digital Seems to Be Paying Off", "kicker": "Advertising ", "content_kicker": null, "print_headline": "At Publicis, a Big Bet on Digital Seems to Be Paying Off", "name": null, "seo