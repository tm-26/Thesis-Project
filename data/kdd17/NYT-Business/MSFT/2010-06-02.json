[{"abstract": "Americans watched 253,652 years worth of videos last month. And you wonder why you feel like you got nothing done?", "web_url": "https://bits.blogs.nytimes.com/2010/06/01/youtube-dominates-online-video-views/", "snippet": "Americans watched 253,652 years worth of videos last month. And you wonder why you feel like you got nothing done?", "lead_paragraph": "Although Google\u2019s purchase of YouTube hasn\u2019t paid of financially, it has clearly made Google a giant in the world of online video, displaying more than 13 billion videos during the month of April.", "source": "The New York Times", "multimedia": [], "headline": {"main": "YouTube Dominates Online Video Views", "kicker": "Bits", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Computers and the Internet", "rank": 1, "major": "N"}, {"name": "subject", "value": "Recordings and Downloads (Video)", "rank": 2, "major": "N"}, {"name": "subject", "value": "Television", "rank": 3, "major": "N"}, {"name": "organizations", "value": "ABC INC", "rank": 4, "major": "N"}, {"name": "organizations", "value": "comScore Inc", "rank": 5, "major": "N"}, {"name": "organizations", "value": "Facebook.com", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Forrester Research Incorporated", "rank": 7, "major": "N"}, {"name": "organizations", "value": "Fox Broadcasting Co", "rank": 8, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 9, "major": "N"}, {"name": "organizations", "value": "Hulu.com", "rank": 10, "major": "N"}, {"name": "organizations", "value": "Microsoft Corp", "rank": 11, "major": "N"}, {"name": "organizations", "value": "MTV Networks", "rank": 12, "major": "N"}, {"name": "organizations", "value": "National Broadcasting Co", "rank": 13, "major": "N"}, {"name": "organizations", "value": "Viacom Inc", "rank": 14, "major": "N"}, {"name": "organizations", "value": "YouTube.com", "rank": 15, "m