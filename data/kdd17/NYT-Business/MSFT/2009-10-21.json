[{"abstract": "Microsoft is expected to announce integration of Twitter and Facebook status updates into Bing.", "web_url": "https://bits.blogs.nytimes.com/2009/10/21/microsofts-bing-to-intergate-twitter-facebook/", "snippet": "Microsoft is expected to announce integration of Twitter and Facebook status updates into Bing.", "lead_paragraph": "Microsoft planned to announce Wednesday that it had fully integrated posts from Twitter into the search results of Bing, according to people with knowledge of the company\u2019s plans. The company also planned to announce that it had integrated Facebook status updates into Bing, according to a person with knowledge of the plans. ", "source": "The New York Times", "multimedia": [], "headline": {"main": "Microsoft\u2019s Bing to Integrate Twitter and Facebook Posts", "kicker": "Bits", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [], "pub_date": "2009-10-21T16:55:54+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By Miguel Helft and Brad Stone", "person": [{"firstname": "Miguel", "middlename": null, "lastname": "Helft", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}, {"firstname": "Brad", "middlename": null, "lastname": "Stone", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 2}], "organization": null}, "type_of_material": "", "_id": "nyt://article/2605e140-9ebc-523f-a0b2-30071b7a66e4", "word_count": 261, "uri": "nyt://article/2605e140-9ebc-523f-a0b2-30071b7a66e4"}, {"abstract": "Microsoft\u2019s new Windows 7 operating system fixes much of what users hated about the much-maligned Vista.", "web_url": "https://www.nytimes.com/2009/10/22/technology/personaltech/22pogue.html", "snippet": "Microsoft\u2019s new Windows 7 operating system fixes much of what users hated about the much-maligned Vista.", "lead_paragraph": "Windows 7 comes out Thursday