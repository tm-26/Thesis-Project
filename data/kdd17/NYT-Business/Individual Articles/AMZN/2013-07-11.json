[{"abstract": "A federal judge\u2019s antitrust finding against Apple over e-book prices underscores how much the book industry has changed and bolsters the advantage held by the online retailer.", "web_url": "https://www.nytimes.com/2013/07/11/business/e-book-ruling-gives-amazon-an-advantage.html", "snippet": "A federal judge\u2019s antitrust finding against Apple over e-book prices underscores how much the book industry has changed and bolsters the advantage held by the online retailer.", "lead_paragraph": "Jeff Bezos, the founder of Amazon, loves disrupting markets. In that regard, he must be having a delightful summer. The book business, once so mired in the past it seemed part of the antiques trade, is up for grabs.", "print_section": "A", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2013/07/11/business/Antitrust/Antitrust-articleLarge.jpg", "height": 369, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2013/07/11/business/Antitrust/Antitrust-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 369}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2013/07/11/business/Antitrust/Antitrust-jumbo.jpg", "height": 629, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2013/07/11/business/Antitrust/Antitrust-superJumbo.jpg", "height": 1258, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}], "headline": {"main": "E-Book Ruling Gives Amazon an Advantage", "kicker": null, "content_kicker": null, "print_headline": "E-Book Ruling Gives Amazon An Advantage", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Book Trade and Publishing", "rank": 1, "major": "N"}, {"name": "perso