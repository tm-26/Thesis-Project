[{"abstract": "The New York attorney general\u2019s office is investigating 13 large retailers over whether increasingly unpredictable and on-call work schedules violate the state\u2019s labor laws.", "web_url": "https://www.nytimes.com/2015/04/13/business/retailers-scrutinized-for-schedules-and-staffing.html", "snippet": "The New York attorney general\u2019s office is investigating 13 large retailers over whether increasingly unpredictable and on-call work schedules violate the state\u2019s labor laws.", "lead_paragraph": "The office of the New York attorney general, Eric Schneiderman, is investigating 13 large retailers over whether increasingly unpredictable and on-call work schedules adopted by some employers in the retail sector violate New York labor laws.", "print_section": "B", "print_page": "3", "source": "The New York Times", "multimedia": [], "headline": {"main": "Retailers Scrutinized for Schedules and Staffing", "kicker": null, "content_kicker": null, "print_headline": "Retailers Scrutinized for Schedules and Staffing", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "glocations", "value": "New York State", "rank": 2, "major": "N"}, {"name": "subject", "value": "Labor and Jobs", "rank": 3, "major": "N"}, {"name": "subject", "value": "Shopping and Retail", "rank": 4, "major": "N"}, {"name": "persons", "value": "Schneiderman, Eric T", "rank": 5, "major": "N"}, {"name": "subject", "value": "Attorneys General", "rank": 6, "major": "N"}], "pub_date": "2015-04-13T04:28:53+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "byline": {"original": "By Hiroko Tabuchi", "person": [{"firstname": "Hiroko", "middlename": null, "lastname": "Tabuchi", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/94191327-e59f-5d47-96c5-399fc7111bfe", "word_count": 292, "uri": "nyt://article/94191327-e59f-5d47-96c5-399fc7111bfe"}, {"abstract": "The C.E.O. has come to the recognition that G.E.\u2019s real strength is industrial engineering, dealing a blow to his predecessor\u2019s initiatives.", "web_url": "https://www.nytimes.com/2015/04/14/business/jeffrey-immelt-with-new-challenges-refashions-jack-welchs-ge.html", "snippet": "The C.E.O. has come to the recognition that G.E.\u2019s real strength is industrial engineering, dealing a blow to his predecessor\u2019s initiatives.", "lead_paragraph": "Jeffrey R. Immelt, chief executive of General Electric, was speaking at Stanford Business School a few years ago when a student asked him pointedly: How tough was it to be the next act at G.E. after the celebrated two-decade tenure of Jack Welch?", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/14/business/14ge-web/14ge-web-articleLarge.jpg", "height": 400, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2015/04/14/business/14ge-web/14ge-web-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 400}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/14/business/14ge-web/14ge-web-jumbo.jpg", "height": 683, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/14/business/14ge-web/14ge-web-superJumbo.jpg", "height": 1152, "width": 1728, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/14/business/14ge-web/14ge-web-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2015/04/14/business/14ge-web/14ge-web-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtype": "thumbLarge", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/14/business/14ge-web/14ge-web-thumbLarge.jpg", "height": 150, "width": 150, "subType": "thumbLarge", "crop_name": "thumbLarge", "legacy": {}}], "headline": {"main": "Jeffrey Immelt Is Putting His Own Stamp on Jack Welch\u2019s G.E.", "kicker": null, "content_kicker": null, "print_headline": "G.E. Boss Sheds Welch\u2019s Shadow in a Retooling", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Immelt, Jeffrey R", "rank": 1, "major": "N"}, {"name": "persons", "value": "Lohr, Steve", "rank": 2, "major": "N"}, {"name": "organizations", "value": "General Electric Company", "rank": 3, "major": "N"}, {"name": "organizations", "value": "GE Capital Corp", "rank": 4, "major": "N"}, {"name": "subject", "value": "Executives and Management (Theory)", "rank": 5, "major": "N"}, {"name": "persons", "value": "Welch, John F Jr", "rank": 6, "major": "N"}], "pub_date": "2015-04-13T17:00:37+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "byline": {"original": "By Steve Lohr", "person": [{"firstname": "Steve", "middlename": null, "lastname": "Lohr", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/d8c82432-69e8-5bdb-a076-38ebb3a6454e", "word_count": 1188, "uri": "nyt://article/d8c82432-69e8-5bdb-a076-38ebb3a6454e"}, {"abstract": "Borrowing by GE Capital has been a huge moneymaker for Wall Street, but much of that business could be going away.", "web_url": "https://www.nytimes.com/2015/04/14/business/dealbook/general-electrics-days-as-wall-streets-golden-goose-may-be-numbered.html", "snippet": "Borrowing by GE Capital has been a huge moneymaker for Wall Street, but much of that business could be going away.", "lead_paragraph": "Wall Street will probably have to bid farewell to its fee darling, General Electric. The conglomerate has been the best corporate client for investment bankers over the last 15 years, spending some $5.1 billion for advice and assistance. Shrinking GE Capital will keep M.&A. practitioners busy for a while. Borrowing by that financial operation, however, has been the biggest moneymaker.", "source": "The New York Times", "multimedia": [], "headline": {"main": "General Electric\u2019s Days as Wall Street\u2019s Golden Goose May Be Numbered", "kicker": "Breakingviews", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Banking and Financial Institutions", "rank": 1, "major": "N"}, {"name": "subject", "value": "Mergers, Acquisitions and Divestitures", "rank": 2, "major": "N"}, {"name": "subject", "value": "Stocks and Bonds", "rank": 3, "major": "N"}, {"name": "organizations", "value": "General Electric Company", "rank": 4, "major": "N"}, {"name": "organizations", "value": "GE Capital Corp", "rank": 5, "major": "N"}], "pub_date": "2015-04-13T20:39:42+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "DealBook", "byline": {"original": "By Antony Currie", "person": [{"firstname": "Antony", "middlename": null, "lastname": "Currie", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/775af018-cd94-581a-bde0-d4e468754e76", "word_count": 355, "uri": "nyt://article/775af018-cd94-581a-bde0-d4e468754e76"}]