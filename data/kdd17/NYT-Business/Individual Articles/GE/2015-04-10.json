[{"abstract": "G.E. will sell most of its finance arm, focusing on its core industrial operations as it completes a transformation begun during the financial crisis.", "web_url": "https://www.nytimes.com/2015/04/11/business/dealbook/general-electric-to-sell-bulk-of-its-finance-unit.html", "snippet": "G.E. will sell most of its finance arm, focusing on its core industrial operations as it completes a transformation begun during the financial crisis.", "lead_paragraph": "For decades, General Electric was happy to reap the enormous profits that arose from its finance arm as it swelled into one of the country\u2019s biggest lenders.", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [], "headline": {"main": "G.E. to Retreat From Finance in Post-Crisis Reorganization", "kicker": null, "content_kicker": null, "print_headline": "General Electric, in a Reorganization, Will Sell Off Its Financial Division", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "de la Merced, Michael J", "rank": 1, "major": "N"}, {"name": "organizations", "value": "General Electric Company", "rank": 2, "major": "N"}, {"name": "persons", "value": "Sorkin, Andrew Ross", "rank": 3, "major": "N"}, {"name": "subject", "value": "Banking and Financial Institutions", "rank": 4, "major": "N"}, {"name": "subject", "value": "Mergers, Acquisitions and Divestitures", "rank": 5, "major": "N"}, {"name": "organizations", "value": "GE Capital Corp", "rank": 6, "major": "N"}, {"name": "subject", "value": "Regulation and Deregulation of Industry", "rank": 7, "major": "N"}, {"name": "organizations", "value": "Blackstone Group, The", "rank": 8, "major": "N"}, {"name": "persons", "value": "Immelt, Jeffrey R", "rank": 9, "major": "N"}, {"name": "organizations", "value": "Centerview Partners", "rank": 10, "major": "N"}, {"name": "organizations", "value": "JPMorgan Chase & Company", "rank": 11, "major": "N"}], "pub_date": "2015-04-10T10:33:25+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "DealBook", "byline": {"original": "By Michael J. de la Merced and Andrew Ross Sorkin", "person": [{"firstname": "Michael", "middlename": "J.", "lastname": "de la Merced", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}, {"firstname": "Andrew", "middlename": "Ross", "lastname": "Sorkin", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 2}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/516f9f7c-8eac-5479-8da9-11ded8a89eae", "word_count": 1400, "uri": "nyt://article/516f9f7c-8eac-5479-8da9-11ded8a89eae"}, {"abstract": "Mr. Welch\u2019s successor, Jeffrey R. Immelt, is now getting out of the banking business and returning G.E. to its industrial roots.", "web_url": "https://www.nytimes.com/2015/04/11/business/dealbook/ge-is-finally-exorcising-financial-demons-from-jack-welch-era.html", "snippet": "Mr. Welch\u2019s successor, Jeffrey R. Immelt, is now getting out of the banking business and returning G.E. to its industrial roots.", "lead_paragraph": "At long last, General Electric is on its way to exorcising the financial demons of Jack Welch.", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/11/business/dbpix-welch/dbpix-welch-articleLarge.jpg", "height": 434, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2015/04/11/business/dbpix-welch/dbpix-welch-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 434}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/11/business/dbpix-welch/dbpix-welch-jumbo.jpg", "height": 741, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/11/business/dbpix-welch/dbpix-welch-superJumbo.jpg", "height": 1481, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/11/business/dbpix-welch/dbpix-welch-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2015/04/11/business/dbpix-welch/dbpix-welch-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtype": "thumbLarge", "caption": null, "credit": null, "type": "image", "url": "images/2015/04/11/business/dbpix-welch/dbpix-welch-thumbLarge.jpg", "height": 150, "width": 150, "subType": "thumbLarge", "crop_name": "thumbLarge", "legacy": {}}], "headline": {"main": "G.E. Is Finally Exorcising Financial Demons From Jack Welch Era", "kicker": "Breakingviews", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Regulation and Deregulation of Industry", "rank": 1, "major": "N"}, {"name": "organizations", "value": "GE Capital Corp", "rank": 2, "major": "N"}, {"name": "organizations", "value": "General Electric Company", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Synchrony Financial", "rank": 4, "major": "N"}, {"name": "persons", "value": "Immelt, Jeffrey R", "rank": 5, "major": "N"}, {"name": "persons", "value": "Welch, John F Jr", "rank": 6, "major": "N"}, {"name": "subject", "value": "Mergers, Acquisitions and Divestitures", "rank": 7, "major": "N"}], "pub_date": "2015-04-10T16:39:45+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "DealBook", "byline": {"original": "By Rob Cox", "person": [{"firstname": "Rob", "middlename": null, "lastname": "Cox", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/4dc599bf-51eb-5de5-a4df-0ec629b73361", "word_count": 580, "uri": "nyt://article/4dc599bf-51eb-5de5-a4df-0ec629b73361"}, {"abstract": "The company\u2019s expected tax bill of $6 billion on $36 billion in repatriated cash could be \u201cmultiples\u201d less than analysts had estimated.", "web_url": "https://www.nytimes.com/2015/04/11/business/dealbook/general-electrics-move-on-overseas-cash-highlights-tax-debate.html", "snippet": "The company\u2019s expected tax bill of $6 billion on $36 billion in repatriated cash could be \u201cmultiples\u201d less than analysts had estimated.", "lead_paragraph": "General Electric is returning to its industrial origins. And some $36 billion of its cash is coming home as well.", "print_section": "B", "print_page": "3", "source": "The New York Times", "multimedia": [], "headline": {"main": "G.E.\u2019s Retrieval of Overseas Cash Highlights Tax Debate", "kicker": null, "content_kicker": null, "print_headline": "G.E.\u2019s Retrieval of Overseas Cash Highlights a U.S. Tax Debate", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Gelles, David", "rank": 1, "major": "N"}, {"name": "subject", "value": "Corporate Taxes", "rank": 2, "major": "N"}, {"name": "persons", "value": "de la Merced, Michael J", "rank": 3, "major": "N"}, {"name": "organizations", "value": "General Electric Company", "rank": 4, "major": "N"}, {"name": "subject", "value": "Federal Taxes (US)", "rank": 5, "major": "N"}, {"name": "organizations", "value": "GE Capital Corp", "rank": 6, "major": "N"}, {"name": "subject", "value": "Tax Shelters", "rank": 7, "major": "N"}], "pub_date": "2015-04-10T17:45:50+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "DealBook", "byline": {"original": "By David Gelles and Michael J. de la Merced", "person": [{"firstname": "David", "middlename": null, "lastname": "Gelles", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}, {"firstname": "Michael", "middlename": "J.", "lastname": "de la Merced", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 2}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/5733aade-90d8-57be-b81f-3301914f3bbf", "word_count": 894, "uri": "nyt://article/5733aade-90d8-57be-b81f-3301914f3bbf"}, {"abstract": "While big banks would like to shed the \u201csystemically important financial institution\u201d tag, they would find it more difficult than General Electric.", "web_url": "https://www.nytimes.com/2015/04/11/business/dealbook/ges-move-is-enticing-to-banks-but-hard-to-copy.html", "snippet": "While big banks would like to shed the \u201csystemically important financial institution\u201d tag, they would find it more difficult than General Electric.", "lead_paragraph": "Jeffrey R. Immelt, the chief executive of General Electric, has plotted an escape for GE Capital from onerous regulation. He is offloading most of the conglomerate\u2019s finance unit, which should allow it to ditch the tag imposed by watchdogs of being a systemically important financial institution, or SIFI. Big United States banks might fancy similar liberty, but they are more tightly hemmed in.", "source": "The New York Times", "multimedia": [], "headline": {"main": "G.E.\u2019s Move Is Enticing to Banks, but Hard to Copy", "kicker": "Breakingviews", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Banking and Financial Institutions", "rank": 1, "major": "N"}, {"name": "organizations", "value": "General Electric Company", "rank": 2, "major": "N"}, {"name": "organizations", "value": "GE Capital Corp", "rank": 3, "major": "N"}, {"name": "subject", "value": "Regulation and Deregulation of Industry", "rank": 4, "major": "N"}], "pub_date": "2015-04-10T19:18:09+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "DealBook", "byline": {"original": "By Antony Currie", "person": [{"firstname": "Antony", "middlename": null, "lastname": "Currie", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/167284ce-9746-5a6b-9b36-7e82e950b550", "word_count": 368, "uri": "nyt://article/167284ce-9746-5a6b-9b36-7e82e950b550"}, {"abstract": "In the crisis of 2008, the weakness of GE Capital was a warning about the risks posed by large financial firms that were not banks.", "web_url": "https://www.nytimes.com/2015/04/11/business/dealbook/the-lessons-for-finance-in-the-ge-capital-retreat.html", "snippet": "In the crisis of 2008, the weakness of GE Capital was a warning about the risks posed by large financial firms that were not banks.", "lead_paragraph": "More than 10 years ago, the kinds of investors who seek out weak companies were circulating presentations on Wall Street that argued that General Electric\u2019s enormous lending business was a ticking time bomb.", "print_section": "B", "print_page": "3", "source": "The New York Times", "multimedia": [], "headline": {"main": "The Lessons for Finance in the GE Capital Retreat", "kicker": null, "content_kicker": null, "print_headline": "In G.E. Sale, End to Age of Hubris", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Eavis, Peter", "rank": 1, "major": "N"}, {"name": "organizations", "value": "General Electric Company", "rank": 2, "major": "N"}, {"name": "organizations", "value": "GE Capital Corp", "rank": 3, "major": "N"}, {"name": "subject", "value": "Banking and Financial Institutions", "rank": 4, "major": "N"}, {"name": "subject", "value": "Regulation and Deregulation of Industry", "rank": 5, "major": "N"}, {"name": "subject", "value": "Dodd-Frank Wall Street Reform and Consumer Protection Act (2010)", "rank": 6,