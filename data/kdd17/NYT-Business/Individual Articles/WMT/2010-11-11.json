[{"abstract": "The rate of unemployment is higher among veterans, and many companies like Microsoft and Wal-Mart are financing job training to close that gap.", "web_url": "https://www.nytimes.com/2010/11/11/giving/11VETS.html", "snippet": "The rate of unemployment is higher among veterans, and many companies like Microsoft and Wal-Mart are financing job training to close that gap.", "lead_paragraph": "MANY veterans are finding that jobs in this difficult economy require skills and training they do not have. Government data shows a higher unemployment rate for recent veterans than for the general population.", "print_section": "F", "print_page": "5", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2010/11/11/giving/VET/VET-jumbo.jpg", "height": 1024, "width": 737, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2010/11/11/giving/VET/VET-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2010/11/11/giving/VET/VET-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Helping Veterans Find Civilian Jobs", "kicker": null, "content_kicker": null, "print_headline": "Helping Veterans Find Civilian Jobs", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Labor and Jobs", "rank": 1, "major": "N"}, {"name": "subject", "value": "Veterans", "rank": 2, "major": "N"}, {"name": "subject", "value": "Philanthropy", "rank": 3, "major": "N"}, {"name": "subject", "value": "Vocational Training", "rank": 4, "major": "N"}], "pub_date": "2010-11-11T01:52:27+0000", "document_type": "article", "news_desk": "Business", "section_name": "Giving", "byline": {"original": "By Elizabeth Olson", "person": [{"firstname": "Elizabeth", "middlename": n