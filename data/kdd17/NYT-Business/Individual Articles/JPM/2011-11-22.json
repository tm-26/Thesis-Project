[{"abstract": "Private banks, which have been slow to embrace mobile technologies, expect to interact more with their clients through social media and smartphone apps.", "web_url": "https://www.nytimes.com/2011/11/23/business/global/with-apps-wealth-management-goes-mobile.html", "snippet": "Private banks, which have been slow to embrace mobile technologies, expect to interact more with their clients through social media and smartphone apps.", "lead_paragraph": "As stock markets continue on their roller-coaster ride, even investors who profess to be buy-and-hold types have become eager users of mobile technology that allows them to track their portfolios almost on a minute-by-minute basis. ", "print_section": "B", "print_page": "3", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/11/23/your-money/23iht-nwapps23-inline/23iht-nwapps23-inline-jumbo.jpg", "height": 1024, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2011/11/23/your-money/23iht-nwapps23-inline/23iht-nwapps23-inline-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2011/11/23/your-money/23iht-nwapps23-inline/23iht-nwapps23-inline-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "With Apps, Wealth Management Goes Mobile", "kicker": "I.H.T. Special Report: Net Worth", "content_kicker": null, "print_headline": "Taking Aim at Tomorrow\u2019s Clients, Wealth Managers Begin to Embrace Mobile Apps", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "UBS A.G", "rank": 1, "major": "N"}, {"name": "subject", "value": "Personal Finances", "rank": 2, "major": "N"}, {"name": "subject", "value": "High Net Worth Individuals", "rank": 3, "m