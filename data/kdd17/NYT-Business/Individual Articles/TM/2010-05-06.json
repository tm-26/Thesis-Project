[{"abstract": "Newly released documents show that some Canadian regulators were just as frustrated by Toyota as were transportation officials in the United States.", "web_url": "https://www.nytimes.com/2010/05/06/business/global/06auto.html", "snippet": "Newly released documents show that some Canadian regulators were just as frustrated by Toyota as were transportation officials in the United States.", "lead_paragraph": "OTTAWA \u2014 During much of the eight-month crisis between Toyota and safety regulators over recalled vehicles in the United States and Canada, the public pronouncements from the two governments have been remarkably different. ", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2010/05/06/business/06auto_CA0/06auto_CA0-articleLarge.jpg", "height": 300, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2010/05/06/business/06auto_CA0/06auto_CA0-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 300}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2010/05/06/business/06auto_CA0/06auto_CA0-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2010/05/06/business/06auto_CA0/06auto_CA0-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Canada\u2019s Doubts on Toyota", "kicker": null, "content_kicker": null, "print_headline": "Canada\u2019s Doubts on Toyota", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Automobile Safety Features and Defects", "rank": 1, "major": "N"}, {"name": "subject", "value": "Regulation and Deregulation of Industry", "rank": 2, "major": "N"}, {"name": "subject", "value": "Automobiles", "rank": 3, "major": "N"}, {"name": "organizations", "value"