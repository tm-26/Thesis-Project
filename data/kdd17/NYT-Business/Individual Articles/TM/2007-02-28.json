[{"abstract": "Toyota\u2019s eighth assembly plant in North America will be in an area best known for being the birthplace of Elvis.", "web_url": "https://www.nytimes.com/2007/02/28/business/28auto.html", "snippet": "Toyota\u2019s eighth assembly plant in North America will be in an area best known for being the birthplace of Elvis.", "lead_paragraph": "Toyota, the Japanese automaker, said yesterday that it would invest $1.3 billion to build its eighth North American assembly plant in Blue Springs, Miss., just outside Tupelo in northeastern Mississippi.", "print_section": "C", "print_page": "3", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2007/02/28/business/28toyota.600.jpg", "height": 280, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2007/02/28/business/28toyota.600.jpg", "xlargewidth": 600, "xlargeheight": 280}}], "headline": {"main": "Toyota to Build $1.3 Billion Plant in the Land of Elvis", "kicker": null, "content_kicker": null, "print_headline": "Toyota to Build $1.3 Billion Plant in the Land of Elvis and Honey", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "glocations", "value": "ALABAMA", "rank": 1, "major": "N"}, {"name": "subject", "value": "FACTORIES AND INDUSTRIAL PLANTS", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Toyota Motor Corp", "rank": 3, "major": "N"}], "pub_date": "2007-02-28T05:00:00+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "byline": {"original": "By Micheline Maynard", "person": [{"firstname": "Micheline", "middlename": null, "lastname": "Maynard", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/455513fe-db35-58d3-a32a-869d4b3c85d6", "word_count": 758, "uri": "nyt://article/455513fe-db35-58d3-a32a-86