[{"abstract": "The grim forecast from Toyota is the latest sign of the global auto industry\u2019s slowdown after years of profits and expansion.", "web_url": "https://www.nytimes.com/2008/12/23/business/23auto.html", "snippet": "The grim forecast from Toyota is the latest sign of the global auto industry\u2019s slowdown after years of profits and expansion.", "lead_paragraph": "A surprisingly grim forecast from Toyota Motor on Monday \u0097 that it will lose money this fiscal year on its vehicle business for the first time in seven decades \u0097 is the latest sign of the global auto industry\u2019s sharp slowdown after years of rising profits and rapid expansion.", "print_section": "A", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2008/12/22/business/22toyota-600.jpg", "height": 331, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2008/12/22/business/22toyota-600.jpg", "xlargewidth": 600, "xlargeheight": 331}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2008/12/22/business/22toyota-75.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2008/12/22/business/22toyota-75.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Car Slump Jolts Toyota, Halting 70 Years of Gain", "kicker": null, "content_kicker": null, "print_headline": "CAR SLUMP JOLTS TOYOTA, HALTING 70 YEARS OF GAIN", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Toyota Motor Corp", "rank": 1, "major": "N"}, {"name": "subject", "value": "Automobiles", "rank": 2, "major": "N"}, {"name": "subject", "value": "Sales", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Honda Motor Co Ltd", "rank": 4, "major": "N"}, {"name": "subject", "value": "Subprime Mortgage Crisis", "rank": 5, "major": "N"}, {"name": "organizations", "value": "Peugeot SA", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Chrysler LLC", "rank": 7, "major": "N"}, {"name": "organizations", "value": "General Motors Corp", "rank": 8, "major": "N"}], "pub_date": "2008-12-23T01:28:04+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "byline": {"original": "By Bill Vlasic and Martin Fackler", "person": [{"firstname": "Bill", "middlename": null, "lastname": "Vlasic", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}, {"firstname": "Martin", "middlename": null, "lastname": "Fackler", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 2}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/b37f03bd-1b2f-5bd3-a65f-e34d64181c67", "word_count": 1070, "uri": "nyt://article/b37f03bd-1b2f-5bd3-a65f-e34d64181c67"}, {"abstract": "Although they have ample credit, foreign-owned carmakers with U.S. operations are facing slowing sales.", "web_url": "https://www.nytimes.com/2008/12/23/business/23transplants.html", "snippet": "Although they have ample credit, foreign-owned carmakers with U.S. operations are facing slowing sales.", "lead_paragraph": "SAN ANTONIO \u2014 Sales are off and production is down, so workers at the Toyota Tundra truck factory here are taking classes: how to handle tools safely, how to get along better with colleagues of varying backgrounds. Some have even cleaned local parks and fed the hungry while collecting Toyota paychecks.", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2008/12/23/business/transplants.1.600.jpg", "height": 300, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2008/12/23/business/transplants.1.600.