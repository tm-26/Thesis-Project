[{"abstract": "A service for nearly four million Verizon FiOS subscribers will allow them to buy items that they see on the History Channel.", "web_url": "https://www.nytimes.com/2011/12/05/business/media/history-channel-and-fios-try-selling-items-instantly-via-tv.html", "snippet": "A service for nearly four million Verizon FiOS subscribers will allow them to buy items that they see on the History Channel.", "lead_paragraph": "Giving television viewers the ability to buy products they see on their screens with a click of a remote control \u2014 say the Manolo Blahniks on Carrie Bradshaw\u2019s feet or Ross\u2019s sweater on \u201cFriends\u201d \u2014 has been talked about by advertisers for years without much success.", "print_section": "B", "print_page": "8", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/12/05/business/05adco/05adco-jumbo.jpg", "height": 701, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2011/12/05/business/05adco/05adco-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2011/12/05/business/05adco/05adco-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "With a Click of the Remote, Impulse Purchases", "kicker": "Advertising", "content_kicker": null, "print_headline": "With a Click of the Remote, Impulse Purchases", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Verizon Communications Inc", "rank": 1, "major": "N"}, {"name": "subject", "value": "ADVERTISING AND MARKETING", "rank": 2, "major": "N"}, {"name": "subject", "value": "Remote Control Systems", "rank": 3, "major": "N"}, {"name": "subject", "value": "Television", "rank": 4, "major": "N"}, {"name": "subject