[{"abstract": "Despite all the hoopla surrounding the Apple announcement, more people buy smartphones running Google\u2019s Android operating system than buy Apple phones.", "web_url": "https://www.nytimes.com/2011/10/04/technology/apple-iphone-faces-divided-loyalties.html", "snippet": "Despite all the hoopla surrounding the Apple announcement, more people buy smartphones running Google\u2019s Android operating system than buy Apple phones.", "lead_paragraph": "SAN FRANCISCO \u2014 As soon as Apple unveils its highly anticipated new version of the iPhone on Tuesday, millions of people are likely to start plotting how to be among the first to buy it. But millions more may be considering a competitor \u2014 an Android phone.", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/04/business/APPLE/APPLE-jumbo.jpg", "height": 683, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/04/business/APPLE/APPLE-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2011/10/04/business/APPLE/APPLE-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Before Latest Phone Debut, Apple Has Harsher Competition", "kicker": null, "content_kicker": null, "print_headline": "Before Its Latest Phone Debut, Apple Faces Harsher Competition", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "iPhone", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Apple Incorporated", "rank": 2, "major": "N"}, {"name": "subject", "value": "Smartphones", "rank": 3, "major": "N"}, {"name": "subject", "value": "Android (Operating System)", "rank": 4, "major": "N"}], "pub_date": "2011-10