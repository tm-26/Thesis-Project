[{"abstract": "Steve Jobs spent his final weeks \u2014 as he had spent most of his life \u2014 in tight control of his choices.", "web_url": "https://www.nytimes.com/2011/10/07/technology/with-time-running-short-steve-jobs-managed-his-farewells.html", "snippet": "Steve Jobs spent his final weeks \u2014 as he had spent most of his life \u2014 in tight control of his choices.", "lead_paragraph": "Over the last few months, a steady stream of visitors to Palo Alto, Calif., called an old friend\u2019s home number and asked if he was well enough to entertain visitors, perhaps for the last time.", "print_section": "A", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/technology/JOBS/JOBS-articleLarge.jpg", "height": 350, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2011/10/07/technology/JOBS/JOBS-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 350}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/technology/JOBS/JOBS-jumbo.jpg", "height": 820, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/technology/JOBS/JOBS-thumbStandard-v2.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2011/10/07/technology/JOBS/JOBS-thumbStandard-v2.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "With Time Running Short, Jobs Managed His Farewells", "kicker": null, "content_kicker": null, "print_headline": "With Time Running Short, Jobs Managed His Farewells", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Computers and the Internet", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Apple Incorporated", "rank": 2, "major": "N"}, {"name": "persons", "value": "Jobs, Steven P", "rank": 3, "major": "N"}], "pub_date": "2011-10-07T01:25:50+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By Charles Duhigg", "person": [{"firstname": "Charles", "middlename": null, "lastname": "Duhigg", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/a559d0e1-f2d6-5e5f-978e-c824745da529", "word_count": 1228, "uri": "nyt://article/a559d0e1-f2d6-5e5f-978e-c824745da529"}, {"abstract": "The tributes to Steve Jobs, the Apple co-founder who died Wednesday, continued to flood social media platforms from around the world on Thursday.", "web_url": "https://mediadecoder.blogs.nytimes.com/2011/10/06/tributes-to-steve-jobs-flood-social-media-platforms/", "snippet": "The tributes to Steve Jobs, the Apple co-founder who died Wednesday, continued to flood social media platforms from around the world on Thursday.", "lead_paragraph": "The tributes to Steve Jobs continued to flood social media platforms from around the world on Thursday with millions of 140-character messages on Twitter, more than 3,000 videos uploaded onto YouTube and tens of thousands of people sharing news links and making comments on Facebook.", "source": "The New York Times", "multimedia": [], "headline": {"main": "Tributes to Steve Jobs Flood Social Media Platforms", "kicker": "Media Decoder", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Social Networking (Internet)", "rank": 1, "major": "N"}, {"name": "persons", "value": "Jobs, Steven P", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Apple Incorporated", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Crimson Hexagon", "rank": 4, "major": "N"}, {"name": "organizations", "value": "Facebook Inc", "rank": 5, "major": "N"}, {"name": "organizations", "value": "Twitter", "rank": 6, "major": "N"}, {"name": "organizations", "value": "YouTube.com", "rank": 7, "major": "N"}], "pub_date": "2011-10-07T01:36:52+0000", "document_type": "article", "news_desk": "", "section_name": "Business Day", "subsection_name": "Media", "byline": {"original": "By Jennifer Preston", "person": [{"firstname": "Jennifer", "middlename": null, "lastname": "Preston", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/4b5e1308-938b-57ff-9d8f-ababbc3c0c3d", "word_count": 514, "uri": "nyt://article/4b5e1308-938b-57ff-9d8f-ababbc3c0c3d"}, {"abstract": "As executives in music, film and publishing learned, a deal with Steve Jobs meant inclusion in one of the most important digital marketplaces on the planet. But the deals were made on Mr. Jobs\u2019s terms.", "web_url": "https://www.nytimes.com/2011/10/07/business/media/master-of-the-media-marketplace-and-its-demanding-gatekeeper.html", "snippet": "As executives in music, film and publishing learned, a deal with Steve Jobs meant inclusion in one of the most important digital marketplaces on the planet. But the deals were made on Mr. Jobs\u2019s terms.", "lead_paragraph": "Last November, Steven P. Jobs finally achieved his long-sought goal of bringing the Beatles to iTunes. But years before he made the deal, he began working on the promotional campaign for the music, personally designing advertisements that would sit in virtual mothballs as long as necessary.", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/business/MEDIA/MEDIA-articleLarge.jpg", "height": 325, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2011/10/07/business/MEDIA/MEDIA-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 325}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/business/MEDIA/MEDIA-jumbo.jpg", "height": 575, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}], "headline": {"main": "Master of the Media Marketplace, and Its Demanding Gatekeeper", "kicker": null, "content_kicker": null, "print_headline": "Moving On, Minus Its Genius: Master of the Media Marketplace, and Its Demanding Gatekeeper", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Jobs, Steven P", "rank": 1, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Apple Incorporated", "rank": 3, "major": "N"}, {"name": "subject", "value": "Media", "rank": 4, "major": "N"}], "pub_date": "2011-10-07T02:04:00+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "Media", "byline": {"original": "By Ben Sisario", "person": [{"firstname": "Ben", "middlename": null, "lastname": "Sisario", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/6ca6950d-06a1-5c38-b2f1-da6c7eac9f66", "word_count": 1121, "uri": "nyt://article/6ca6950d-06a1-5c38-b2f1-da6c7eac9f66"}, {"abstract": "In Steve Jobs\u2019s efforts to reshape the high-tech industry, he gained a reputation for making often cutting comments about competitors and to his own staff.", "web_url": "https://www.nytimes.com/2011/10/07/technology/steve-jobs-defended-his-work-with-a-barbed-tongue.html", "snippet": "In Steve Jobs\u2019s efforts to reshape the high-tech industry, he gained a reputation for making often cutting comments about competitors and to his own staff.", "lead_paragraph": "SAN FRANCISCO \u2014 The first time Steve Jobs ever bullied anyone was in the third grade. He and some pals \u201cbasically destroyed\u201d the teacher, he once said.", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/business/STYLE/STYLE-jumbo.jpg", "height": 953, "width": 686, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/business/STYLE/STYLE-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2011/10/07/business/STYLE/STYLE-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Defending Life\u2019s Work With Words of a Tyrant", "kicker": null, "content_kicker": null, "print_headline": "Moving On, Minus Its Genius: Defending Life\u2019s Work With Words Of a Tyrant", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Apple Incorporated", "rank": 1, "major": "N"}, {"name": "persons", "value": "Jobs, Steven P", "rank": 2, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 3, "major": "N"}], "pub_date": "2011-10-07T02:17:20+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By David Streitfeld", "person": [{"firstname": "David", "middlename": null, "lastname": "Streitfeld", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/b6670a1e-5136-5c79-a415-d572559fc5e6", "word_count": 811, "uri": "nyt://article/b6670a1e-5136-5c79-a415-d572559fc5e6"}, {"abstract": "Apple pioneered the big budget Super Bowl ad on TV, just one of the marketing advances led by Steven P. Jobs, executives at Advertising Week 2011 said Thursday.", "web_url": "https://www.nytimes.com/2011/10/07/business/media/recalling-apples-influence-and-innovations-advertising.html", "snippet": "Apple pioneered the big budget Super Bowl ad on TV, just one of the marketing advances led by Steven P. Jobs, executives at Advertising Week 2011 said Thursday.", "lead_paragraph": "STEVEN P. JOBS would be an influential figure in advertising even if Apple\u2019s sole contribution had been the \u201c1984\u201d commercial that introduced the Macintosh, which pioneered the idea of big-concept, big-budget spots created especially for the Super Bowl.", "print_section": "B", "print_page": "6", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/business/Adco/Adco-jumbo.jpg", "height": 670, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}], "headline": {"main": "Apple Innovations Include Start of Big-Budget Super Bowl Ads", "kicker": "Advertising", "content_kicker": null, "print_headline": "Apple Innovations Include Start Of Big-Budget Super Bowl Ads", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Jobs, Steven P", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Apple Incorporated", "rank": 2, "major": "N"}, {"name": "subject", "value": "ADVERTISING AND MARKETING", "rank": 3, "major": "N"}, {"name": "subject", "value": "Super Bowl", "rank": 4, "major": "N"}], "pub_date": "2011-10-07T02:25:31+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "Media", "byline": {"original": "By Stuart Elliott and Tanzina Vega", "person": [{"firstname": "Stuart", "middlename": null, "lastname": "Elliott", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}, {"firstname": "Tanzina", "middlename": null, "lastname": "Vega", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 2}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/9dcf4a90-146d-56b8-bdc9-fa6e29865aee", "word_count": 949, "uri": "nyt://article/9dcf4a90-146d-56b8-bdc9-fa6e29865aee"}, {"abstract": "Apple could try to remake TV technology next as it embarks on its future without its resident genius, Steven P. Jobs, who died Wednesday.", "web_url": "https://www.nytimes.com/2011/10/07/technology/apple-may-be-aiming-to-makeover-television-next.html", "snippet": "Apple could try to remake TV technology next as it embarks on its future without its resident genius, Steven P. Jobs, who died Wednesday.", "lead_paragraph": "Heard of Apple TV? A lot of people have not. That is because even though Steven P. Jobs helped upend so many businesses with a string of groundbreaking products in music, mobile phones, games and tablet computers, he barely made a dent in television. ", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/business/UNFINISHED2/UNFINISHED2-jumbo.jpg", "height": 1024, "width": 941, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2011/10/07/business/UNFINISHED2/UNFINISHED2-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2011/10/07/business/UNFINISHED2/UNFINISHED2-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Moving On, Minus Its Genius", "kicker": null, "content_kicker": null, "print_headline": "Moving On, Minus Its Genius; Bringing the Apple Magic to Television Remains an Alluring Challenge", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Jobs, Steven P", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Apple Incorporated", "rank": 2, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 3, "major": "N"}, {"name": "subject", "value": "Television Sets", "rank": 4, "major": "N"}], "pub_date": "2011-10-07T03:04:41+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By Nick Wingfield and Amy Chozick", "person": [{"firstname": "Nick", "middlename": null, "lastname": "Wingfield", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}, {"firstname": "Amy", "middlename": null, "lastname": "Chozick", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 2}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/69d575ea-f28c-584c-ad43-5c93951c5263", "word_count": 1087, "uri": "nyt://article/69d575ea-f28c-584c-ad43-5c93951c5263"}, {"abstract": "What Steve Jobs understood that others did not was that design works from the inside out \u2014 it\u2019s the core of the product, not just its outer shell.", "web_url": "https://www.nytimes.com/2011/10/08/business/how-steve-jobs-infused-passion-into-a-commodity.html", "snippet": "What Steve Jobs understood that others did not was that design works from the inside out \u2014 it\u2019s the core of the product, not just its outer shell.", "lead_paragraph": "In the early 1990s Compaq Computer was the technology darling of the day, and PC sales were surging. Dell was promoting its build-on-demand model, Gateway computer shipped its products in boxes with Holstein cow markings, and I.B.M. had introduced the ThinkPad with its Little Tramp marketing campaign. Apple\u2019s Macintosh was introduced during the 1984 Super Bowl but was considered a marginal outlier with its quirky proprietary operating system.", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2011/06/25/business/james-stewart/james-stewart-articleLarge.jpg", "height": 400, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2011/06/25/business/james-stewart/james-stewart-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 400}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/06/25/business/james-stewart/james-stewart-jumbo-v2.jpg", "height": 682, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/06/25/business/james-stewart/james-stewart-superJumbo.jpg", "height": 1333, "width": 2000, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2011/06/25/business/james-stewart/james-stewart-thumbStandard-v3.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2011/06/25/business/james-stewart/james-stewart-thumbStandard-v3.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtype": "thumbLarge", "caption": null, "credit": null, "type": "image", "url": "images/2011/06/25/business/james-stewart/james-stewart-thumbLarge-v2.jpg", "height": 150, "width": 150, "subType": "thumbLarge", "crop_name": "thumbLarge", "legacy": {}}], "headline": {"main": "How Jobs Put Passion Into Products", "kicker": "Common Sense", "content_kicker": null, "print_headline": "How Jobs Put Passion Into Products", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Apple Incorporated", "rank": 1, "major": "N"}, {"name": "persons", "value": "Jobs, Steven P", "rank": 2, "major": "N"}], "pub_date": "2011-10-07T14:05:27+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "byline": {"original": "By James B. Stewart", "person": [{"firstname": "James", "middlename": null, "lastname": "", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/82bfeb64-279f-5047-b093-590789e0629c", "word_count": 1473, "uri": "nyt://article/82bfeb64-279f-5047-b093-590789e0629c"}, {"abstract": "Lee Clow, an advertising executive who worked for decades with Apple on commercials like the iconic \u201c1984\u201d Super Bowl ad, has paid tribute to Steven P.  Jobs, who died on Wednesday.", "web_url": "https://mediadecoder.blogs.nytimes.com/2011/10/07/apples-longtime-ad-partner-reflects-on-jobs/", "snippet": "Lee Clow, an advertising executive who worked for decades with Apple on commercials like the iconic \u201c1984\u201d Super Bowl ad, has paid tribute to Steven P.  Jobs, who died on Wednesday.", "lead_paragraph": "Lee Clow, who worked for decades with Steven P. Jobs and Apple at agencies that include Chiat/Day, TBWA/Chiat/Day and the TBWA/Media Arts Lab unit devoted to Apple, has paid tribute to Mr. Jobs, who died on Wednesday.", "source": "The New York Times", "multimedia": [], "headline": {"main": "Apple\u2019s Longtime Ad Partner Reflects on Jobs", "kicker": "Media Decoder", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "ADVERTISING AND MARKETING", "rank": 1, "major": "N"}, {"name": "persons", "value": "Clow, Lee", "rank": 2, "major": "N"}, {"name": "persons", "value": "Jobs, Steven P", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Apple Incorporated", "rank": 4, "major": "N"}, {"name": "organizations", "value": "TBWA Worldwide", "rank": 5, "major": "N"}, {"name": "organizations", "value": "TBWA/Chiat/Day", "rank": 6, "major": "N"}], "pub_date": "2011-10-07T17:47:33+0000", "document_type": "article", "news_desk": "", "section_name": "Business Day", "subsection_name": "Media", "bylin