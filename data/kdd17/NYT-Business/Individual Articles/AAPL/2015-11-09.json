[{"abstract": "A class action firm that routinely targets tech companies is opening an office in San Francisco, not far from many of the companies it has sued.", "web_url": "https://bits.blogs.nytimes.com/2015/11/09/law-firm-loathed-by-tech-expands-to-industrys-home-turf/", "snippet": "A class action firm that routinely targets tech companies is opening an office in San Francisco, not far from many of the companies it has sued.", "lead_paragraph": "Edelson PC is a class action law firm that specializes in suing tech companies for alleged privacy violations. The firm has sued pretty much every big tech firm you\u2019ve heard of \u2014 Google, Facebook, Apple, Amazon, Netflix \u2014 so Silicon Valley hates them.", "source": "The New York Times", "multimedia": [], "headline": {"main": "Law Firm Loathed by Tech Expands to Industry\u2019s Home Turf", "kicker": "Bits", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Computers and the Internet", "rank": 1, "major": "N"}, {"name": "subject", "value": "Legal Profession", "rank": 2, "major": "N"}, {"name": "subject", "value": "Privacy", "rank": 3, "major": "N"}, {"name": "subject", "value": "Suits and Litigation (Civil)", "rank": 4, "major": "N"}, {"name": "persons", "value": "Edelson, Jay (1972- )", "rank": 5, "major": "N"}, {"name": "organizations", "value": "Facebook Inc", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 7, "major": "N"}, {"name": "organizations", "value": "Twitter", "rank": 8, "major": "N"}], "pub_date": "2015-11-09T10:30:28+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By Conor Dougherty", "person": [{"firstname": "Conor", "middlename": null, "lastname": "Dougherty", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article