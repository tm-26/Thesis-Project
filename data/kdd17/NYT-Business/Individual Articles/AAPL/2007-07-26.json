[{"abstract": "The results exceeded analysts\u2019 expectations as the company reported strong sales of iPods and Macintosh computers, as well as the sale of 270,000 iPhones.", "web_url": "https://www.nytimes.com/2007/07/26/business/26apple.html", "snippet": "The results exceeded analysts\u2019 expectations as the company reported strong sales of iPods and Macintosh computers, as well as the sale of 270,000 iPhones.", "lead_paragraph": "SAN FRANCISCO, July 25 \u2014 Apple on Wednesday reported a 73 percent jump in quarterly profit on strong sales of Macs and iPods, beating Wall Street forecasts. It also alleviated some concerns about early sales of the iPhone.", "print_section": "C", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2007/07/26/business/26apple.600.jpg", "height": 280, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2007/07/26/business/26apple.600.jpg", "xlargewidth": 600, "xlargeheight": 280}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2007/07/26/business/26apple.75.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2007/07/26/business/26apple.75.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Apple Profit Soars 73% as Sales Rise", "kicker": null, "content_kicker": null, "print_headline": "Apple Profit Soars 73% As Sales Rise", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Apple Inc", "rank": 1, "major": "N"}, {"name": "subject", "value": "Company Reports", "rank": 2, "major": "N"}, {"name": "subject", "value": "iPhone", "rank": 3, "major": "N"}, {"name": "subject", "value": "Sales", "rank": 4, "major": "N"}, {"name": "persons", "value": "Jobs, Steven P", "rank": 5, "major": "N"}], "pub_date": "200