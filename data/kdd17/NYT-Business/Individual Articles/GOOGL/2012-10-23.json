[{"abstract": "Sixty percent of 16- through 29-year-olds surveyed by the Pew Research Center said they still visited the library.", "web_url": "https://mediadecoder.blogs.nytimes.com/2012/10/22/young-people-frequent-libraries-study-finds/", "snippet": "Sixty percent of 16- through 29-year-olds surveyed by the Pew Research Center said they still visited the library.", "lead_paragraph": "In a digital world where many younger readers feel increasingly comfortable downloading novels and textbooks onto their computers or e-readers, a majority of Americans from the ages of 16 through 29 still frequent libraries.", "print_section": "B", "print_page": "8", "source": "The New York Times", "multimedia": [], "headline": {"main": "Young People Frequent Libraries, Study Finds", "kicker": "Media Decoder", "content_kicker": null, "print_headline": "Young Americans Frequent Libraries, Pew Study Finds", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Libraries and Librarians", "rank": 1, "major": "N"}, {"name": "subject", "value": "Polls and Public Opinion", "rank": 2, "major": "N"}, {"name": "subject", "value": "Research", "rank": 3, "major": "N"}, {"name": "subject", "value": "Youth", "rank": 4, "major": "N"}, {"name": "organizations", "value": "Gates, Bill and Melinda, Foundation", "rank": 5, "major": "N"}, {"name": "organizations", "value": "Google Book Search", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Pew Research Center", "rank": 7, "major": "N"}], "pub_date": "2012-10-23T00:18:16+0000", "document_type": "article", "news_desk": "", "section_name": "Business Day", "subsection_name": "Media", "byline": {"original": "By Christine Haughney", "person": [{"firstname": "Christine", "middlename": null, "lastname": "Haughney", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/76037d5c-1ef0-5f57-954a-f467be444f18", "word_count": 365, "uri": "nyt://article/76037d5c-1ef0-5f57-954a-f467be444f18"}, {"abstract": "Companies like Google, Microsoft and Intel are scrambling to reinvent their businesses now that the old model \u2014 a customer sitting at a PC \u2014 no longer applies.", "web_url": "https://www.nytimes.com/2012/10/23/technology/in-mobile-world-tech-giants-struggle-to-get-up-to-speed.html", "snippet": "Companies like Google, Microsoft and Intel are scrambling to reinvent their businesses now that the old model \u2014 a customer sitting at a PC \u2014 no longer applies.", "lead_paragraph": "SAN FRANCISCO \u2014 Intel made its fortune on the chips that power personal computers, and Microsoft on the software that goes inside. Google\u2019s secret sauce is that it finds what you are looking for on the Internet. But the ground is shifting beneath these tech titans because of a major force: the rise of mobile devices. ", "print_section": "A", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2012/10/23/business/Mobile1/Mobile1-articleLarge.jpg", "height": 370, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2012/10/23/business/Mobile1/Mobile1-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 370}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/10/23/business/Mobile1/Mobile1-jumbo.jpg", "height": 737, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/10/23/business/Mobile1/Mobile1-superJumbo.jpg", "height": 1473, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2012/10/23/business/Mobile1/Mobile1-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2012/10/23/business/Mobile1/Mobile1-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtype": "thumbLarge", "caption": null, "credit": null, "type": "image", "url": "images/2012/10/23/business/Mobile1/Mobile1-thumbLarge.jpg", "height": 150, "width": 150, "subType": "thumbLarge", "crop_name": "thumbLarge", "legacy": {}}], "headline": {"main": "In Mobile World, Tech Giants Scramble to Get Up to Speed", "kicker": null, "content_kicker": null, "print_headline": "In Mobile World, Tech Giants Scramble to Get Up to Speed", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Intel Corporation", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 2, "major": "N"}, {"name": "subject", "value": "Tablet Computers", "rank": 3, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 4, "major": "N"}, {"name": "subject", "value": "Smartphones", "rank": 5, "major": "N"}, {"name": "organizations", "value": "Microsoft Corporation", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Apple Inc", "rank": 7, "major": "N"}, {"name": "subject", "value": "Mobile Applications", "rank": 8, "major": "N"}, {"name": "organizations", "value": "Facebook Inc", "rank": 9, "major": "N"}], "pub_date": "2012-10-23T01:36:13+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By Claire Cain Miller and Somini Sengupta", "person": [{"firstname": "Claire", "middlename": "Cain", "lastname": "Miller", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}, {"firstname": "Somini", "middlename": null, "lastname": "Sengupta", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 2}], "organization": null}, "type_of_material": "News", "_id": "n