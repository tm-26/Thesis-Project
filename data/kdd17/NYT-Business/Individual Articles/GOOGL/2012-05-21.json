[{"abstract": "A law professor, Eugene Volokh, argues in a paper that Google is a publisher, with First Amendment protection. But sometimes it benefits Google to be seen as a mere connector.", "web_url": "https://www.nytimes.com/2012/05/21/business/media/eugene-volokh-ucla-professor-makes-a-case-for-google-as-publisher.html", "snippet": "A law professor, Eugene Volokh, argues in a paper that Google is a publisher, with First Amendment protection. But sometimes it benefits Google to be seen as a mere connector.", "lead_paragraph": "IS Google search an intermediary like the phone company \u2014 simply connecting people with the information they seek? Or is Google search a publisher, like a newspaper, which provides only the information that it sees fit and is protected by the First Amendment?", "print_section": "B", "print_page": "3", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2012/05/21/business/21link/21link-articleLarge.jpg", "height": 330, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2012/05/21/business/21link/21link-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 330}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/05/21/business/21link/21link-jumbo.jpg", "height": 674, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/05/21/business/21link/21link-superJumbo.jpg", "height": 1348, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2012/05/21/business/21link/21link-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2012/05/21/business/21link/21link-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtype": "thumbLarge", "caption": null, "credit": null, "type": "image", "url": "images/2012/05/21/business/21link/21link-thumbLarge.jpg", "height": 150, "width": 150, "subType": "thumbLarge", "crop_name": "thumbLarge", "legacy": {}}], "headline": {"main": "Professor Makes the Case That Google Is a Publisher", "kicker": "Link by Link", "content_kicker": null, "print_headline": "Professor Makes the Case That Google Is a Publisher", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Freedom of Speech and Expression", "rank": 1, "major": "N"}, {"name": "persons", "value": "Volokh, Eugene", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 3, "major": "N"}, {"name": "subject", "value": "Search Engines", "rank": 4, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 5, "major": "N"}, {"name": "persons", "value": "Schmidt, Eric E", "rank": 6, "major": "N"}, {"name": "subject", "value": "First Amendment (US Constitution)", "rank": 7, "major": "N"}], "pub_date": "2012-05-21T02:39:38+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "Media", "byline": {"original": "By Noam Cohen", "person": [{"firstname": "Noam", "middlename": null, "lastname": "Cohen", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/0888ebdf-5c07-5583-9c0f-22959d4e7050", "word_count": 1171, "uri": "nyt://article/0888ebdf-5c07-5583-9c0f-22959d4e7050"}, {"abstract": "The European Commission warned Google to propose changes in \u201ca matter of weeks\u201d to its method of answering user queries, or possibly face an antitrust lawsuit.", "web_url": "https://www.nytimes.com/2012/05/22/business/global/europe-warns-google-over-antitrust.html", "snippet": "The European Commission warned Google to propose changes in \u201ca matter of weeks\u201d to its method of answering user queries, or possibly face an antitrust lawsuit.", "lead_paragraph": "BRUSSELS \u2014 Google may have abused its dominance in Internet search by promoting its own businesses at the expense of competitors, the European Commission said Monday. It warned the company to propose changes in \u201ca matter of weeks\u201d to its method of answering user queries, or possibly face an antitrust lawsuit.", "print_section": "A", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2012/05/22/technology/22google-web/22google-web-articleLarge.jpg", "height": 330, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2012/05/22/technology/22google-web/22google-web-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 330}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/05/22/technology/22google-web/22google-web-jumbo.jpg", "height": 710, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/05/22/technology/22google-web/22google-web-superJumbo.jpg", "height": 1421, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2012/05/22/technology/22google-web/22google-web-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2012/05/22/technology/22google-web/22google-web-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtype": "thumbLarge", "caption": null, "credit": null, "type": "image", "u