[{"abstract": "Google provided a first look at its Internet-connected glasses, which place a small see-through display screen above a person\u2019s eye.", "web_url": "https://www.nytimes.com/2012/04/05/technology/google-offers-look-at-internet-connected-glasses.html", "snippet": "Google provided a first look at its Internet-connected glasses, which place a small see-through display screen above a person\u2019s eye.", "lead_paragraph": "SAN FRANCISCO \u2014 Google on Wednesday offered a look at a previously secret project to develop Internet-connected glasses, staking out a lead position in a futuristic and fast-growing area known as wearable computing. ", "print_section": "B", "print_page": "3", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/04/05/business/Googlejp/Googlejp-jumbo.jpg", "height": 1024, "width": 835, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2012/04/05/business/Googlejp/Googlejp-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2012/04/05/business/Googlejp/Googlejp-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "A Rose-Colored View May Come Standard", "kicker": null, "content_kicker": null, "print_headline": "A Rose-Colored View May Come Standard", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Eyeglasses", "rank": 1, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 3, "major": "N"}, {"name": "persons", "value": "DeVaul, Richard W", "rank": 4, "major": "N"}, {"name": "subject", "value": "Science and Technology", "rank": 5, "major": "N"}], "pub_date": "2012-04-05T02:27:09+0