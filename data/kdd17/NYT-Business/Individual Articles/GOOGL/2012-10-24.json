[{"abstract": "Regulators on Wednesday said Microsoft had failed to live up to an agreement to give Windows software users better access to rival Internet browsers.", "web_url": "https://www.nytimes.com/2012/10/25/technology/european-antitrust-officials-charge-microsoft-with-violation.html", "snippet": "Regulators on Wednesday said Microsoft had failed to live up to an agreement to give Windows software users better access to rival Internet browsers.", "lead_paragraph": "BRUSSELS \u2014 European regulators on Wednesday charged Microsoft with an antitrust violation for failing to live up to a prior agreement to give users of its Windows software equal access to rival Internet browsers.", "print_section": "B", "print_page": "5", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/10/25/business/Microsoft/Microsoft-jumbo.jpg", "height": 898, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/10/25/business/Microsoft/Microsoft-superJumbo.jpg", "height": 1797, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}], "headline": {"main": "European Antitrust Officials Say Microsoft Violated Deal", "kicker": null, "content_kicker": null, "print_headline": "European Regulator Says Microsoft Violated a Deal", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Google Inc", "rank": 1, "major": "N"}, {"name": "organizations", "value": "European Union", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Mozilla Foundation", "rank": 3, "major": "N"}, {"name": "subject", "value": "Antitrust Laws and Competition Issues", "rank": 4, "major": "N"}, {"name": "organizations", "value": "Microsoft Corporation", "rank": 5, "major": "N"}, {"name": "subject", "value": "Windows (Operating System)"