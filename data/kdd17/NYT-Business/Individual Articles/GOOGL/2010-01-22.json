[{"abstract": "Large companies in a variety of industries have started buying storage straight from Seagate to deal with their exploding volumes of data.", "web_url": "https://bits.blogs.nytimes.com/2010/01/21/seagate-goes-direct/", "snippet": "Large companies in a variety of industries have started buying storage straight from Seagate to deal with their exploding volumes of data.", "lead_paragraph": "Some companies \u2014 most notably Dell \u2014 choose to go direct, others have directness thrust upon them. ", "source": "The New York Times", "multimedia": [], "headline": {"main": "Seagate Goes Direct", "kicker": "Bits", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Google Inc", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Seagate Technology", "rank": 2, "major": "N"}], "pub_date": "2010-01-22T00:44:51+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By Ashlee Vance", "person": [{"firstname": "Ashlee", "middlename": null, "lastname": "Vance", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "", "_id": "nyt://article/14431517-281d-5534-b21b-5f49466fefe2", "word_count": 516, "uri": "nyt://article/14431517-281d-5534-b21b-5f49466fefe2"}, {"abstract": "Nokia\u2019s addition of navigation software on its smartphones is an effort to match Google, which offers free GPS on phones using its Android system.", "web_url": "https://www.nytimes.com/2010/01/22/technology/companies/22nokia.html", "snippet": "Nokia\u2019s addition of navigation software on its smartphones is an effort to match Google, which offers free GPS on phones using its Android system.", "lead_paragraph": "Nokia began giving away professional GPS navigation software on 10 of its smartphones on Thursday, matching a competitive move by Google. The move deals a blow to the leaders in the market for specialized navigation devices, Garmin and TomTom.", "print_section": "B", "print_page": "2", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2010/01/22/business/22nokia_CA0/thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2010/01/22/business/22nokia_CA0/thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Nokia Fires Back at Google With Free GPS on 10 Phones", "kicker": null, "content_kicker": null, "print_headline": "Nokia Fires Back at Google With Free GPS on 10 Phones", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Nokia Oyj", "rank": 1, "major": "N"}, {"name": "subject", "value": "Software", "rank": 2, "major": "N"}, {"name": "subject", "value": "Wireless Communications", "rank": 3, "major": "N"}, {"name": "subject", "value": "Navigation", "rank": 4, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 5, "major": "N"}, {"name": "subject", "value": "Global Positioning System", "rank": 6, "major": "N"}, {"name": "subject", "value": "Cellular Telephones", "rank": 7, "major": "N"}], "pub_date": "2010-01-22T01:56:58+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "subsection_name": "Companies", "byline": {"original": "By Kevin J. O'Brien", "person": [{"firstname": "Kevin", "middlename": "J.", "lastname": "O'Brien", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/deba5dd3-04de-541c-8c60-2846048aaa84", "word_count": 546, "uri": "nyt://article/deba5dd3-04de-541c-8c60-2846048aaa84"}, {"abstract": "In reality Google\u2019s problem in China is relatively small potatoes compared with the challenge of spending its money wisely.", "web_url": "https://www.nytimes.com/2010/01/22/business/economy/22views.html", "snippet": "In reality Google\u2019s problem in China is relatively small potatoes compared with the challenge of spending its money wisely.", "lead_paragraph": "Cash, not China, is Google\u2019s biggest conundrum. More precisely, where should the search giant point its gusher of greenbacks? The online advertising market recovery and increasing efficiency pushed free cash flow up 44 percent, to $2.5 billion in the fourth quarter. Adding that to the company\u2019s $24 billion cash hoard doesn\u2019t make sense. But giving it to shareholders does.", "print_section": "B", "print_page": "2", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2010/01/22/business/22views_CA0/thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2010/01/22/business/22views_CA0/thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "A Good Place for Google\u2019s Cash", "kicker": "Reuters Breakingviews", "content_kicker": null, "print_headline": "A Good Place for Google\u2019s Cash", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "glocations", "value": "China", "rank": 1, "major": "N"}, {"name": "subject", "value": "Mergers, Acquisitions and Divestitures", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 3, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 4, "major": "N"}], "pub_date": "2010-01-22T02:42:52+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "Economy", "byline": {"original": "By Robert Cyran and Martin Hutchinson", "person": [{"firstname": "Robert", "middlename": null, "lastname": "Cyran", "qualifier": null, "title": null