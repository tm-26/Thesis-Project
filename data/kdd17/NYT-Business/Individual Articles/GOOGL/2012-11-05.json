[{"abstract": "Publishers in Europe and South America have joined anew in the fight against Google, arguing that it is picking their pockets every time it links to articles.", "web_url": "https://www.nytimes.com/2012/11/05/business/media/google-news-faces-challenges-from-publishers-abroad.html", "snippet": "Publishers in Europe and South America have joined anew in the fight against Google, arguing that it is picking their pockets every time it links to articles.", "lead_paragraph": "They say you should never take on people who spill ink by the barrel, but your odds are better when you traffic in terabytes of data. In the United States, Google and big media went at it for several years over Google News and Google won, taking its argument for a free and open Internet all the way to the bank.", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2012/06/12/business/media/carr-new-headshot/carr-new-headshot-articleLarge.jpg", "height": 900, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2012/06/12/business/media/carr-new-headshot/carr-new-headshot-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 900}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/06/12/business/media/carr-new-headshot/carr-new-headshot-jumbo-v2.jpg", "height": 1024, "width": 683, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/06/12/business/media/carr-new-headshot/carr-new-headshot-superJumbo-v2.jpg", "height": 2048, "width": 1366, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2012/06/12/business/media/carr-new-headshot/carr-new-headshot-thumbStandard-v3.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2012/06/12/business/media/carr-new-headshot/carr-new-headshot-thumbStandard-v3.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtype": "thumbLarge", "caption": null, "credit": null, "type": "image", "url": "images/2012/06/12/business/media/carr-new-headshot/carr-new-headshot-thumbLarge-v3.jpg", "height": 150, "width": 150, "subType": "thumbLarge", "crop_name": "thumbLarge", "legacy": {}}], "headline": {"main": "Publishers Abroad Take On Google", "kicker": "The Media Equation", "content_kicker": null, "print_headline": "Publishers Abroad Take On Google", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Newspapers", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Google News", "rank": 3, "major": "N"}, {"name": "glocations", "value": "Europe", "rank": 4, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 5, "major": "N"}, {"name": "glocations", "value": "South America", "rank": 6, "major": "N"}, {"name": "subject", "value": "News and News Media", "rank": 7, "major": "N"}], "pub_date": "2012-11-05T00:30:59+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "Media", "byline": {"original": "By David Carr", "person": [{"firstname": "David", "middlename": null, "lastname": "Carr", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/639399c8-5468-503b-8d7a-c21a6aef9e56", "word_count": 1149, "uri": "nyt://article/639399c8-5468-503b-8d7a-c21a6aef9e56"}, {"abstract": "Also, overseas publishers take on Google News, the continuing dispute over online music royalties, and BBC executives who missed opportunities to learn more about allegations of sexual abuse against Jimmy Savile.", "web_url": "https://mediadecoder.blogs.nytimes.com/2012/11/05/the-breakfast-meeting-restraint-on-election-day-and-revenge-against-amazon/", "snippet": "Also, overseas publishers take on Google News, the continuing dispute over online music royalties, and BBC executives who missed opportunities to learn more about allegations of sexual abuse against Jimmy Savile.", "lead_paragraph": "Executives at the TV networks and other news gathering operations said they were committed to holding off proclaiming a winner in the presidential election on Tuesday until they were confident of a winner, Brian Stelter reported. The promised restraint shows that the networks are fully aware that Web sites and individuals on social media services like Twitter and Facebook undoubtedly would be sharing results as soon as they come in, rushing to call the race.", "source": "The New York Times", "multimedia": [], "headline": {"main": "The Breakfast Meeting: Restraint on Election Day, and Revenge Against Amazon", "kicker": "Media Decoder", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Beck, Glenn", "rank": 1, "major": "N"}, {"name": "persons", "value": "Ferriss, Timothy", "rank": 2, "major": "N"}, {"name": "persons", "value": "Schmidt, Eric E", "rank": 3, "major": "N"}, {"name": "persons", "value": "Thompson, Mark John (1957- )", "rank": 4, "major": "N"}, {"name": "organizations", "value": "AMAZON.COM INC", "rank": 5, "major": "N"}, {"name": "organizations", "value": "British Broadcasting Corp", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 7, "major": "N"}, {"name": "organizations", "value": "Pandora Media Inc", "rank": 8, "major": "N"}], "pub_date": "2012-11-05T15:33:15+0000", "document_type": "article", "news_desk": "", "section_name": "Business Day", "subsection_name": "Media", "byline": 