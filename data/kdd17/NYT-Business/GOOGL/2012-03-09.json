[{"abstract": "A team at Google is reimagining four classic advertising campaigns with the goal of demonstrating that new-media ads can create an emotional appeal.", "web_url": "https://www.nytimes.com/2012/03/09/business/media/google-remixes-four-classic-campaigns-advertising.html", "snippet": "A team at Google is reimagining four classic advertising campaigns with the goal of demonstrating that new-media ads can create an emotional appeal.", "lead_paragraph": "GOOGLE is going all Marty McFly on Madison Avenue, sponsoring a back-to-the-future project that reimagines classic campaigns from the \u201cMad Men\u201d age for modern sensibilities \u2014 and technologies.", "print_section": "B", "print_page": "4", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2012/03/09/business/Adco1/Adco1-articleLarge.jpg", "height": 252, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2012/03/09/business/Adco1/Adco1-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 252}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2012/03/09/business/Adco1/Adco1-jumbo.jpg", "height": 430, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}], "headline": {"main": "Google Remixes Old Campaigns, Adding a Dash of Digital Tools", "kicker": "Advertising", "content_kicker": null, "print_headline": "Google Remixes Old Campaigns, Adding a Dash Of Digital Tools", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Coca-Cola Company", "rank": 1, "major": "N"}, {"name": "subject", "value": "Online Advertising", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Volvo Car Corp", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 4, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 5, "m