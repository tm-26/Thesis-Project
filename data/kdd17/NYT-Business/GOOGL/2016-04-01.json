[{"abstract": "Even if you did not download the music to your iOS device from the iTunes store, you can still take it along on your mobile gadget.", "web_url": "https://www.nytimes.com/2016/04/02/technology/personaltech/moving-cd-ripped-music-from-the-computer-to-the-ipad.html", "snippet": "Even if you did not download the music to your iOS device from the iTunes store, you can still take it along on your mobile gadget.", "lead_paragraph": "Q. I have many CDs that I purchased from various retail stores (not Apple iTunes) and loaded into my iTunes library on my computer. Is it possible to download these albums to my iPhone and iPad? If so, how?", "source": "The New York Times", "multimedia": [], "headline": {"main": "Moving CD-Ripped Music From the Computer to the iPad", "kicker": "Tech Tip", "content_kicker": null, "print_headline": "", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Music", "rank": 1, "major": "N"}, {"name": "subject", "value": "Audio Recordings, Downloads and Streaming", "rank": 2, "major": "N"}, {"name": "subject", "value": "Mobile Applications", "rank": 3, "major": "N"}, {"name": "subject", "value": "iOS (Operating System)", "rank": 4, "major": "N"}, {"name": "subject", "value": "Data Storage", "rank": 5, "major": "N"}, {"name": "organizations", "value": "AMAZON.COM INC", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Apple Inc", "rank": 7, "major": "N"}, {"name": "organizations", "value": "Google Music", "rank": 8, "major": "N"}, {"name": "organizations", "value": "Google Play", "rank": 9, "major": "N"}, {"name": "organizations", "value": "iTunes", "rank": 10, "major": "N"}], "pub_date": "2016-04-01T13:40:16+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "subsection_name": "Personal Tech", "byline": {"original": "By J. D. Biersdorfer", "person": [{"firstname": "J.", "middlename": null, "lastname": "", "qualifier": null, "title": null, "role": "reported