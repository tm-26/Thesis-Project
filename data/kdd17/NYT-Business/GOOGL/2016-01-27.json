[{"abstract": "Virtual assistants from tech behemoths including Apple, Microsoft, Amazon and Google are graded on their abilities to actually be of assistance.", "web_url": "https://www.nytimes.com/2016/01/28/technology/personaltech/siri-alexa-and-other-virtual-assistants-put-to-the-test.html", "snippet": "Virtual assistants from tech behemoths including Apple, Microsoft, Amazon and Google are graded on their abilities to actually be of assistance.", "lead_paragraph": "WHEN I asked Alexa earlier this week who was playing in the Super Bowl, she responded, somewhat monotonously, \u201cSuper Bowl 49\u2019s winner is New England Patriots.\u201d", "print_section": "B", "print_page": "8", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2016/01/28/technology/personaltech/28techfix-illo/28techfix-illo-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2016/01/28/technology/personaltech/28techfix-illo/28techfix-illo-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}, {"rank": 0, "subtype": "thumbLarge", "caption": null, "credit": null, "type": "image", "url": "images/2016/01/28/technology/personaltech/28techfix-illo/28techfix-illo-thumbLarge.jpg", "height": 150, "width": 150, "subType": "thumbLarge", "crop_name": "thumbLarge", "legacy": {}}, {"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2016/01/28/technology/personaltech/28techfix-illo/28techfix-illo-articleLarge.jpg", "height": 1300, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2016/01/28/technology/personaltech/28techfix-illo/28techfix-illo-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 1300}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2016/01/28/technology/personaltech/28techfix-illo/28techfix-illo-jumbo.jpg", "height": 1024, "width": 473, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2016/01/28/technology/personaltech/28techfix-illo/28techfix-illo-superJumbo.jpg", "height": 2048, "width": 946, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}], "headline": {"main": "Siri, Alexa and Other Virtual Assistants Put to the Test", "kicker": "Tech Fix", "content_kicker": null, "print_headline": "Putting Virtual Assistants to the Test", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Chen, Brian X", "rank": 1, "major": "N"}, {"name": "subject", "value": "Artificial Intelligence", "rank": 2, "major": "N"}, {"name": "organizations", "value": "AMAZON.COM INC", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Apple Inc", "rank": 4, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 5, "major": "N"}, {"name": "organizations", "value": "Microsoft Corp", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Alphabet Inc", "rank": 7, "major": "N"}, {"name": "subject", "value": "Mobile Applications", "rank": 8, "major": "N"}, {"name": "organizations", "value": "Facebook Inc", "rank": 9, "major": "N"}, {"name": "subject", "value": "Voice Recognition Systems", "rank": 10, "major": "N"}], "pub_date": "2016-01-27T10:00:07+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "subsection_name": "Personal Tech", "byline": {"original": "By Brian X. Chen", "person": [{"firstname": "Brian", "middlename": "X.", "lastname": "Chen", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/bc016b7c-2a80-5277-83e6-e81c4550c5b3", "word_count": 1344, "uri": "nyt://article/bc016b7c-2a80-5277-83e6-e81c4550c5b3"}, {"abstract": "In order to be of use, assistants like Cortana and Siri need to know a lot about you. Reading the companies\u2019 privacy statements can give you an idea of how much.", "web_url": "https://www.nytimes.com/2016/01/28/technology/personaltech/protecting-personal-information-from-virtual-assistants.html", "snippet": "In order to be of use, assistants like Cortana and Siri need to know a lot about you. Reading the companies\u2019 privacy statements can give you an idea of how much.", "lead_paragraph": "Q. I was going to try Cortana, Microsoft\u2019s version of Siri, but to use it, you have to allow Microsoft access to your location, your contacts, voice input, information from email and text messages, browser history, search history, calendar details \u201cand other information.\u201d Naturally, I declined. Why does Microsoft need so much personal information and what does Microsoft do with it?", "print_section": "B", "print_page": "8", "source": "The New York Times", "multimedia": [], "headline": {"main": "Protecting Personal Information From Virtual Assistants", "kicker": "Tech Tip", "content_kicker": null, "print_headline": "The Data Needs of Virtual Assistants", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Artificial Intelligence", "rank": 1, "major": "N"}, {"name": "persons", "value": "Biersdorfer, J D", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Microsoft Corp", "rank": 3, "major": "N"}, {"name": "subject", "value": "Privacy", "rank": 4, "major": "N"}, {"name": "subject", "value": "Software", "rank": 5, "major": "N"}, {"name": "organizations", "value": "Siri Inc", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Apple Inc", "rank": 7, "major": "N"}, {"name": "organizations", "value": "Twitter", "rank": 8, "major": "N"}, {"name": "organizations", "value": "Google Now", "rank": 9, "major": "N"}, {"name": "organizations", "value": "Facebook Inc", "rank": 10, "major": "N"}], "pub_date": "2016-01-27T14:52:52+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "subsection_name": "Personal Tech", "byline": {"original": "By J. D. Biersdorfer", "person": [{"firstname": "J.", "middlename": null, "lastname": "", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "Question", "_id": "nyt://article/16ba5e72-6a89-5509-8724-719a3da40ba8", "word_count": 327, "uri": "nyt://article/16ba5e72-6a89-5509-8724-719a3da40ba8"}, {"abstract": "The technology company agreed to pay $185 million on revenue generated in a 10-year period, a deal the government called a \u201cmajor success.\u201d", "web_url": "https://www.nytimes.com/2016/01/28/business/international/british-tax-deal-with-google-puts-government-on-defensive.html", "snippet": "The technology company agreed to pay $185 million on revenue generated in a 10-year period, a deal the government called a \u201cmajor success.\u201d", "lead_paragraph": "LONDON \u2014 When it was announced last Friday, Google\u2019s agreement to pay 130 million pounds in back taxes was welcomed by the country\u2019s chancellor of the Exchequer, George Osborne, as a \u201cmajor success.\u201d", "print_section": "B", "print_page": "2", "source": "The New York Times", "multimedia": [], "headline": {"main": "British Tax Deal With Google Puts Government on Defensive", "kicker": null, "content_kicker": null, "print_headline": "Business Briefing; Cameron Defends Tax Deal With Google", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Castle, Stephen", "rank": 1, "major": "N"}, {"name": "subject", "value": "Corporate Taxes", "rank": 2, "major": "N"}, {"name": "glocations", "value": "Great Britain", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 4, "major": "N"}, {"name": "persons", "value": "Cameron, David", "rank": 5, "major": "N"}], "pub_date": "2016-01-27T18:04:41+0000