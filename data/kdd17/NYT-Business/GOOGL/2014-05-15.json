[{"abstract": "Europe\u2019s highest court defended its citizens\u2019 right to privacy. But it left unstated what the channels and particulars are.", "web_url": "https://www.nytimes.com/2014/05/15/technology/one-court-ruling-on-privacy-in-europe-and-28-regulators.html", "snippet": "Europe\u2019s highest court defended its citizens\u2019 right to privacy. But it left unstated what the channels and particulars are.", "lead_paragraph": "LONDON \u2014 The ruling on Tuesday by Europe\u2019s highest court that Google can be forced to remove links from certain searches will be carried out by data privacy regulators at 28 different agencies across the European Union.", "print_section": "B", "print_page": "1", "source": "The New York Times", "multimedia": [], "headline": {"main": "One Court Ruling on Privacy in Europe, and 28 Regulators", "kicker": null, "content_kicker": null, "print_headline": "One Ruling on Privacy, 28 Agencies", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "subject", "value": "Privacy", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 3, "major": "N"}, {"name": "organizations", "value": "European Union", "rank": 4, "major": "N"}, {"name": "organizations", "value": "European Court of Justice", "rank": 5, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 6, "major": "N"}, {"name": "subject", "value": "Search Engines", "rank": 7, "major": "N"}], "pub_date": "2014-05-15T00:48:12+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By Mark Scott", "person": [{"firstname": "Mark", "middlename": null, "lastname": "Scott", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News Analysis", "_id": "nyt://article/10852606-61e2-517a-8f5e-8117e81c0492", "word_count": 766, "uri": "nyt://article/10852606-61e2-517a-8f5e-8117e81c0492"}, {"abstract": "A basic online tool for people to ask Google to take down potential harmful links would be in place in about two weeks, a top German regulator said.", "web_url": "https://bits.blogs.nytimes.com/2014/05/15/after-european-court-decision-google-works-on-a-link-removal-tool/", "snippet": "A basic online tool for people to ask Google to take down potential harmful links would be in place in about two weeks, a top German regulator said.", "lead_paragraph": "LONDON \u2013 Google will announce by the end of the month a mechanism for consumers to request that links to information about them be removed from the company\u2019s search engine, a leading European regulator said on Thursday. It was one of the first signs that Google was working through how to operate after a court ruling said consumers could make such requests.", "print_section": "B", "print_page": "6", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "xlarge", "caption": null, "credit": null, "type": "image", "url": "images/2014/05/15/business/15bits-google/15bits-google-articleLarge.jpg", "height": 364, "width": 600, "subType": "xlarge", "crop_name": "articleLarge", "legacy": {"xlarge": "images/2014/05/15/business/15bits-google/15bits-google-articleLarge.jpg", "xlargewidth": 600, "xlargeheight": 364}}, {"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2014/05/15/business/15bits-google/15bits-google-jumbo.jpg", "height": 622, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "superJumbo", "caption": null, "credit": null, "type": "image", "url": "images/2014/05/15/business/15bits-google/15bits-google-superJumbo.jpg", "height": 1243, "width": 2048, "subType": "superJumbo", "crop_name": "superJumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2014/05/15/business/15bits-google/15bits-google-thumbStandard.jpg", "height": 75, "width":