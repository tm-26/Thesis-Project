[{"abstract": "Facebook, Google, Twitter and Microsoft announced they have teamed up to fight the spread of such content online by sharing technology and information.", "web_url": "https://www.nytimes.com/2016/12/05/technology/facebook-and-other-tech-companies-seek-to-curb-flow-of-terrorist-content.html", "snippet": "Facebook, Google, Twitter and Microsoft announced they have teamed up to fight the spread of such content online by sharing technology and information.", "lead_paragraph": "SAN FRANCISCO \u2014 For all the good that has come from the internet, the online world has also served as a powerful device for recruiting terrorists and spreading their propaganda.", "print_section": "B", "print_page": "4", "source": "The New York Times", "multimedia": [], "headline": {"main": "Facebook and Other Tech Companies Seek to Curb Flow of Terrorist Content", "kicker": null, "content_kicker": null, "print_headline": "Facebook and Other Tech Companies Seek to Curb the Flow of Terrorist Content", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "Facebook Inc", "rank": 1, "major": "N"}, {"name": "subject", "value": "Computers and the Internet", "rank": 3, "major": "N"}, {"name": "subject", "value": "Terrorism", "rank": 4, "major": "N"}, {"name": "subject", "value": "Propaganda", "rank": 5, "major": "N"}, {"name": "subject", "value": "Social Media", "rank": 6, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 7, "major": "N"}, {"name": "organizations", "value": "Microsoft Corp", "rank": 8, "major": "N"}, {"name": "organizations", "value": "Twitter", "rank": 9, "major": "N"}], "pub_date": "2016-12-06T03:04:09+0000", "document_type": "article", "news_desk": "Business", "section_name": "Technology", "byline": {"original": "By Mike Isaac", "person": [{"firstname": "Mike", "middlename": null, "lastname": "Isaac", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null},