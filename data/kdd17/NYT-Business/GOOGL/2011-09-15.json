[{"abstract": "The three major technology companies have traditionally competed for digital advertising revenue, but they will now sell ads for one another.", "web_url": "https://www.nytimes.com/2011/09/15/business/media/aol-yahoo-and-microsoft-are-said-to-reach-an-ad-deal.html", "snippet": "The three major technology companies have traditionally competed for digital advertising revenue, but they will now sell ads for one another.", "lead_paragraph": "Yahoo, AOL and Microsoft, three major technology companies that have traditionally competed for digital advertising revenue, have created an unusual partnership in which they will sell ads for one another. ", "print_section": "B", "print_page": "3", "source": "The New York Times", "multimedia": [], "headline": {"main": "AOL, Yahoo and Microsoft Reportedly in Ad Deal", "kicker": null, "content_kicker": null, "print_headline": "AOL, Yahoo And Microsoft Reportedly In Ad Deal", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "organizations", "value": "AOL", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Yahoo! Inc", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 3, "major": "N"}, {"name": "subject", "value": "Online Advertising", "rank": 4, "major": "N"}, {"name": "organizations", "value": "Microsoft Corporation", "rank": 5, "major": "N"}], "pub_date": "2011-09-15T00:39:03+0000", "document_type": "article", "news_desk": "Business", "section_name": "Business Day", "subsection_name": "Media", "byline": {"original": "By Tanzina Vega", "person": [{"firstname": "Tanzina", "middlename": null, "lastname": "Vega", "qualifier": null, "title": null, "role": "reported", "organization": "", "rank": 1}], "organization": null}, "type_of_material": "News", "_id": "nyt://article/2059ffb0-b6cf-5fec-971f-3bf98188da2f", "word_count": 482, "uri": "nyt://article/2059ffb0-b6cf-5fec-971f-3bf98188da2f"}, {"abstract": "His job today remains essentially the same as it was a decade ago: fetch the highest price for technology clients.", "web_url": "https://www.nytimes.com/2011/09/15/business/frank-quattrones-golden-touch-at-qatalyst.html", "snippet": "His job today remains essentially the same as it was a decade ago: fetch the highest price for technology clients.", "lead_paragraph": "When Frank Quattrone comes calling, buyers should beware. The latest coup for the former Credit Suisse First Boston ace is persuading Google to bid against itself for Motorola Mobility. Since starting Qatalyst Partners, his boutique investment bank, Mr. Quattrone has extracted a much higher premium for clients than rival tech bankers. Silicon Valley has now been given fair warning.", "print_section": "B", "print_page": "2", "source": "The New York Times", "multimedia": [{"rank": 0, "subtype": "jumbo", "caption": null, "credit": null, "type": "image", "url": "images/2011/09/15/business/Views/Views-jumbo.jpg", "height": 973, "width": 1024, "subType": "jumbo", "crop_name": "jumbo", "legacy": {}}, {"rank": 0, "subtype": "thumbnail", "caption": null, "credit": null, "type": "image", "url": "images/2011/09/15/business/Views/Views-thumbStandard.jpg", "height": 75, "width": 75, "subType": "thumbnail", "crop_name": "thumbStandard", "legacy": {"thumbnail": "images/2011/09/15/business/Views/Views-thumbStandard.jpg", "thumbnailwidth": 75, "thumbnailheight": 75}}], "headline": {"main": "Frank Quattrone\u2019s Golden Touch", "kicker": "Reuters BreakingViews", "content_kicker": null, "print_headline": "Frank Quattrone\u2019s Golden Touch", "name": null, "seo": null, "sub": null}, "keywords": [{"name": "persons", "value": "Quattrone, Frank P", "rank": 1, "major": "N"}, {"name": "organizations", "value": "Qatalyst Partners", "rank": 2, "major": "N"}, {"name": "organizations", "value": "Motorola Mobility Inc", "rank": 3, "major": "N"}, {"name": "organizations", "value": "Google Inc", "rank": 4, "major": "N"}, {"name": "glocations", "value": "China", "rank": 5, "major": "N"}, {"name":